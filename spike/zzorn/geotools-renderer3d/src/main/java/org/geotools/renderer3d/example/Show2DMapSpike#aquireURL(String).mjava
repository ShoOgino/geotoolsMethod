    public static URL aquireURL( String target )
    {
        if ( new File( target ).exists() )
        {
            try
            {
                return new File( target ).toURI().toURL();
            }
            catch ( MalformedURLException e )
            {
            }
        }
        try
        {
            return new URL( target );
        }
        catch ( MalformedURLException e )
        {
            return null;
        }
    }

