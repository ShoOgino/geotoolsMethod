    public void handleClass(Class c) {
        Method[] methods = c.getDeclaredMethods();
        for (int t = 0; t < methods.length; t++) {
            try {
                Method method = methods[t];
                PrintStream ps = System.out;

                // emitHeader(method,ps);
                emitCode(method, ps);
                // emitFooter(method,ps);

            } catch (Exception e) {
                e.printStackTrace();
            }
        }
    }

