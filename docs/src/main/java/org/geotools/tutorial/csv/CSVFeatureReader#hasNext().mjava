    /**
     * Check if additional content is available.
     *
     * @return <code>true</code> if additional content is available
     * @throws IOException
     */
    public boolean hasNext() throws IOException {
        if (next != null) {
            return true;
        } else {
            next = readFeature(); // read next feature so we can check
            return next != null;
        }
    }
