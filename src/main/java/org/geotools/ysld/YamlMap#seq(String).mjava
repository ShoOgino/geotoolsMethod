    /**
     * Access value as a YamlSeq
     * 
     * @param key
     * @return Access as YamlSeq
     */
    public YamlSeq seq(String key) {
        Object obj = raw.get(key);
        if (obj == null) {
            return null;
        }
        if( obj instanceof YamlSeq){
            return (YamlSeq) obj;
        }
        return new YamlSeq(obj);
    }

