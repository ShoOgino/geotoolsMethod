    SymbolsEncoder encode(String key, Stroke stroke) {
        if (stroke != null) {
            push(key);
            put("color", stroke.getColor());
            put("width", stroke.getColor());
            put("opacity", nullIf(stroke.getColor(), 1d));
            pop();
//            join:
//            cap:
//            dash:
//            dash-offset:
//            graphic-fill: ?
//            graphic-stroke: ?
        }
        return this;
    }

