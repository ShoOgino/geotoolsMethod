        YamlObj style = transform("point", "stroke.sld");

        YamlObj mark =
            style.o("feature-styles", 0).o("rules", 0).o("symbolizers", 0).o("point").o("symbols", 0).o("mark");
        assertEquals("circle", mark.s("shape"));

