    @Test
    public void testEmptyTitle() throws Exception {
        SLDParser parser = new SLDParser(styleFactory, input(EmptyTitleSLD));
        Style[] styles = parser.readXML();
        assertEquals(
                "",
                styles[0]
                        .featureTypeStyles()
                        .get(0)
                        .rules()
                        .get(0)
                        .getDescription()
                        .getTitle()
                        .toString());
    }

