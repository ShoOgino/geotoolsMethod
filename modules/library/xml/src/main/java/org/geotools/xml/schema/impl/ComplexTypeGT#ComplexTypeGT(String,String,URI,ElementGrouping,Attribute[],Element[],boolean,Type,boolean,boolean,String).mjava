    /** Construct <code>ComplexTypeGT</code>. */
    public ComplexTypeGT(
            String id,
            String name,
            URI namespace,
            ElementGrouping child,
            Attribute[] attrs,
            Element[] elems,
            boolean mixed,
            Type parent,
            boolean _abstract,
            boolean derived,
            String anyAttributeNS) {
        this.name = name;
        this.id = id;
        this.anyAtrNS = anyAttributeNS;
        this.ns = namespace;
        this.parent = parent;
        this.attrs = attrs;
        this.elems = elems;
        this.child = child;
        this._abstract = _abstract;
        this.mixed = mixed;
        this.derived = derived;
    }

