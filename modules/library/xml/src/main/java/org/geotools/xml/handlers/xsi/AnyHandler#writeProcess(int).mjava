    /**
     * reverses the findProcess method, converting from integers to String for the process
     * attribute.
     *
     * @param process
     */
    public static String writeProcess(int process) {
        switch (process) {
            case LAX:
                return "lax";

            case SKIP:
                return "skip";

            case STRICT:
            default:
                return "strict";
        }
    }

