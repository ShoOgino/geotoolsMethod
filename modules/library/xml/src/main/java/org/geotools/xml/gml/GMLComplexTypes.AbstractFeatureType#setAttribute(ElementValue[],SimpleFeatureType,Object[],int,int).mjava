        private void setAttribute(
                ElementValue[] value, SimpleFeatureType ft, Object[] values, int i, int j) {
            j = searchByName(value, ft, i, j);
            if (j != -1) {
                assignValue(value, values, ft.getDescriptor(j), i, j);
            } else {
                searchByType(value, ft, values, i);
            }
        }

