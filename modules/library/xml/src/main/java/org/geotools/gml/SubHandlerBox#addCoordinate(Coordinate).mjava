    /**
     * Sets a corner.
     *
     * @param c the coordinate of the corner.
     */
    @Override
    public void addCoordinate(Coordinate c) {
        LOGGER.entering("SubHandlerBox", "addCoordinate", c);
        e.expandToInclude(c);
        LOGGER.exiting("SubHandlerBox", "addCoordinate");
    }

