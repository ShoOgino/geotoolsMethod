        /** @see org.geotools.xml.xsi.ElementGrouping#findChildElement(java.lang.String) */
        public Element findChildElement(String name) {
            if (elements == null) {
                return null;
            }

            for (int i = 0; i < elements.length; i++) {
                Element t = elements[i].findChildElement(name);

                if (t != null) { // found it

                    return t;
                }
            }

            return null;
        }

