        /** @see org.geotools.xml.xsi.ElementGrouping#findChildElement(java.lang.String) */
        public Element findChildElement(String name) {
            if (children == null) {
                return null;
            }

            for (ElementGrouping child : children) {
                Element t = child.findChildElement(name);

                if (t != null) { // found it

                    return t;
                }
            }

            return null;
        }

