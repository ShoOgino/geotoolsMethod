    /** @see org.geotools.xml.schema.ElementGrouping#findChildElement(java.lang.String) */
    @Override
    public Element findChildElement(String name) {
        if (children != null) {
            for (ElementGrouping child : children) {
                Element e = child.findChildElement(name);

                if (e != null) {
                    return e;
                }
            }
        }

        return null;
    }

