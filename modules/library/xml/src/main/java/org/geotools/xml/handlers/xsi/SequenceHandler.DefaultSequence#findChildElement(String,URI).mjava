        public Element findChildElement(String localName, URI namespaceURI) {
            if (children == null) {
                return null;
            }

            for (ElementGrouping child : children) {
                Element t = child.findChildElement(localName, namespaceURI);

                if (t != null) { // found it

                    return t;
                }
            }

            return null;
        }

