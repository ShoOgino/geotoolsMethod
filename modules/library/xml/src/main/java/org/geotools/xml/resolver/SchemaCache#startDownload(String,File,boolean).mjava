    /**
     * Adds the given file / location to the list of files in download.
     * 
     * @param location
     * @param file
     */
    private void startDownload(String location, File file, boolean isTemp) {
        if(!isTemp) {
            synchronized(locationsInDownload) {
                locationsInDownload.add(location);                
            }
        }
    }

