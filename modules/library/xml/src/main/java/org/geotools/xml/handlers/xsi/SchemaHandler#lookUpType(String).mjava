    /**
     * convinience method for package
     *
     * @param qname
     * @throws SAXException
     */
    protected Type lookUpType(String qname) throws SAXException {
        if (qname == null) return null;
        Type t = null;
        t = lookUpComplexType(qname);
        t = t == null ? lookUpSimpleType(qname) : t;
        return t;
    }

