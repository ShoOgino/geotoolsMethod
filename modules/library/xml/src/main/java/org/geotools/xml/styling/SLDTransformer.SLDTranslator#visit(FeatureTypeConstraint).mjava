        @Override
        public void visit(FeatureTypeConstraint ftc) {
            start("FeatureTypeConstraint");

            if (ftc != null) {
                element("FeatureTypeName", ftc.getFeatureTypeName());
                visit(ftc.getFilter());

                Extent[] extent = ftc.getExtents();

                for (Extent value : extent) {
                    visit(value);
                }
            }

            end("FeatureTypeConstraint");
        }

