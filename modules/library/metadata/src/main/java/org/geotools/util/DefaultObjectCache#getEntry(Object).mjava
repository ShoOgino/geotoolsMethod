    /**
     * Retrieve cache entry, will create one if needed.
     *
     * @return ObjectCacheEntry
     */
    private ObjectCacheEntry getEntry(Object key) {
        synchronized (cache) {
            ObjectCacheEntry entry = (ObjectCacheEntry) cache.get(key);
            if (entry == null) {
                entry = new ObjectCacheEntry();
                cache.put(key, entry);
            }
            return entry;
        }
    }

