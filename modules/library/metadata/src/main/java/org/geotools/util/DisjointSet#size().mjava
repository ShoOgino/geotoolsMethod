    /**
     * Returns the number of elements in this set. The size of this set may change as a result of
     * adding elements to a mutually exclusive set.
     */
    public int size() {
        synchronized (map) {
            int count = 0;
            for (DisjointSet<E> es : map.values()) {
                if (es == this) {
                    count++;
                }
            }
            return count;
        }
    }

