    /**
     * Check if an entry exists in the cache.
     *
     * @return boolean
     */
    public boolean containsKey(final K key) {
        if (cache.containsKey(key)) {
            WeakReference<V> reference = cache.get(key);
            if (reference == null) return false;
            return reference.get() != null;
        }
        return false;
    }

