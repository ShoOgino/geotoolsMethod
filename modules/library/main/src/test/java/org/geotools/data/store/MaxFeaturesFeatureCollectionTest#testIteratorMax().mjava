    public void testIteratorMax() throws Exception {
        MaxSimpleFeatureCollection max = new MaxSimpleFeatureCollection(delegate, 2);
        try (SimpleFeatureIterator i = max.features()) {
            for (int x = 0; x < 2; x++) {
                assertTrue(i.hasNext());
                i.next();
            }
            assertFalse(i.hasNext());
        }
    }

