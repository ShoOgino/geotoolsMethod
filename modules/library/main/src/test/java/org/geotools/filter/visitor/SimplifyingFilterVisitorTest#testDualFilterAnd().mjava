    @Test
    public void testDualFilterAnd() {
        Filter original =
                ff.and(
                        Arrays.asList(
                                ff.not(ff.equal(ff.property("a"), ff.literal(3), true)),
                                ff.equal(ff.property("a"), ff.literal(3), true)));
        Assert.assertEquals(Filter.EXCLUDE, original.accept(simpleVisitor, null));
    }

