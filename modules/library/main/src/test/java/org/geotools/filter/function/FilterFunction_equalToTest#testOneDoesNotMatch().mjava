    @Test
    public void testOneDoesNotMatch() throws Exception {
        FilterFactoryImpl ff = new FilterFactoryImpl();
        Function func =
                ff.function(
                        "equalTo",
                        ff.literal(Arrays.asList(1, 2, 3)),
                        ff.literal(Arrays.asList(1, 2)),
                        ff.literal("ONE"));
        assertFalse((Boolean) func.evaluate(new Object()));
    }

