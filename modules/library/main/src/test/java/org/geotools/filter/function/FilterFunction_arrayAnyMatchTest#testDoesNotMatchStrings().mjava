    @Test
    public void testDoesNotMatchStrings() throws Exception {
        FilterFactoryImpl ff = new FilterFactoryImpl();
        Function func =
                ff.function(
                        "arrayAnyMatch",
                        ff.literal(new String[] {"a", "b", "c", "d"}),
                        ff.literal("g"));
        assertFalse((Boolean) func.evaluate(new Object()));
    }

