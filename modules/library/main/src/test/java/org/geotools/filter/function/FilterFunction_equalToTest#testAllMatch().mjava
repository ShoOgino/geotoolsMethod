    @Test
    public void testAllMatch() throws Exception {
        FilterFactoryImpl ff = new FilterFactoryImpl();
        Function func =
                ff.function(
                        "equalTo",
                        ff.literal(Arrays.asList(1, 1, 1)),
                        ff.literal(Arrays.asList(1)),
                        ff.literal("ALL"));
        assertTrue((Boolean) func.evaluate(new Object()));
    }

