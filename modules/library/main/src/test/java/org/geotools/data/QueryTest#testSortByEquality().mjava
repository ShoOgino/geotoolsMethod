    @Test
    public void testSortByEquality() {
        Query q1 = new Query();

        Query q2 = new Query();
        q2.setSortBy(new SortBy[] {SortBy.NATURAL_ORDER});
        assertThat(q1, not(equalTo(q2)));

        Query q3 = new Query();
        q3.setSortBy(new SortBy[] {SortBy.NATURAL_ORDER});
        assertThat(q2, equalTo(q3));
    }

