    @Test
    public void testDualFilterOr() {
        Or or =
                ff.or(
                        Arrays.asList(
                                ff.not(ff.equal(ff.property("a"), ff.literal(3), true)),
                                ff.equal(ff.property("a"), ff.literal(3), true)));
        Assert.assertEquals(Filter.INCLUDE, or.accept(simpleVisitor, null));
    }

