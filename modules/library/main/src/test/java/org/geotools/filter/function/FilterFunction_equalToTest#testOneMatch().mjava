    @Test
    public void testOneMatch() throws Exception {
        FilterFactoryImpl ff = new FilterFactoryImpl();
        Function func =
                ff.function(
                        "equalTo",
                        ff.literal(Arrays.asList(1, 2, 3)),
                        ff.literal(Arrays.asList(1, 4, 5)),
                        ff.literal("ONE"));
        assertTrue((Boolean) func.evaluate(new Object()));
    }

