    @Test
    public void testAnyMatchArrayAndProperty() throws Exception {
        FilterFactoryImpl ff = new FilterFactoryImpl();
        Function func =
                ff.function(
                        "equalTo",
                        ff.property("value"),
                        ff.literal(new Integer[] {1, 2, 3}),
                        ff.literal("ANY"));
        assertTrue((Boolean) func.evaluate(feature(new Integer[] {1, 2, 3, 4, 5, 6, 7, 8})));
    }

