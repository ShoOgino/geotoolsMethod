    @Test
    public void testAnyDoesNotMatch() throws Exception {
        FilterFactoryImpl ff = new FilterFactoryImpl();
        Function func =
                ff.function(
                        "equalTo",
                        ff.literal(Arrays.asList(4, 5, 6, 7, 8)),
                        ff.literal(Arrays.asList(1, 2, 3)),
                        ff.literal("ANY"));
        assertFalse((Boolean) func.evaluate(new Object()));
    }

