    private void setAll(ScreenMap map, boolean value) {
        for (int x = xmin; x < width; x++) {
            for (int y = ymin; y < height; y++) {
                map.set(x, y, value);
            }
        }

        for (int x = xmin; x < width; x++) {
            for (int y = ymin; y < height; y++) {
                Assert.assertEquals(value, map.get(x, y));
            }
        }
    }

