    public void testGroupByAverageInteger() {
        GroupByVisitor visitor =
                new GroupByVisitor(
                        Aggregate.AVERAGE,
                        ff.property("id"),
                        Arrays.asList(ff.property("id")),
                        new NullProgressListener());
        assertEquals(
                Arrays.asList(Double.class), getResultTypes(visitor, Integer.class, Integer.class));
    }

