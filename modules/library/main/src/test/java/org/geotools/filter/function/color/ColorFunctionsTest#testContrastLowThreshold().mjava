    @Test
    public void testContrastLowThreshold() {
        Color color = new HSLColor(90, 1, 0.5).toRGB();
        Function function =
                FF.function(
                        "contrast",
                        FF.literal(color),
                        FF.literal("#000000"),
                        FF.literal("#ffffff"),
                        FF.literal("30%"));
        Color result = (Color) function.evaluate(null);
        assertEquals("#000000", Converters.convert(result, String.class));
    }

