    @Test
    public void testContrastDarkLigthReference() {
        Function function =
                FF.function(
                        "contrast",
                        FF.literal("#222222"),
                        FF.literal("#101010"),
                        FF.literal("#dddddd"));
        Color result = (Color) function.evaluate(null);
        assertEquals("#DDDDDD", Converters.convert(result, String.class));
    }

