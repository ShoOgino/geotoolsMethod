    private AttributeDescriptor createAttribute(String name, Class clazz) {
        AttributeType at =
                new AttributeTypeImpl(
                        new NameImpl(name),
                        clazz,
                        false,
                        false,
                        Collections.emptyList(),
                        null,
                        null);
        return new AttributeDescriptorImpl(at, new NameImpl(name), 0, 1, false, null);
    }

