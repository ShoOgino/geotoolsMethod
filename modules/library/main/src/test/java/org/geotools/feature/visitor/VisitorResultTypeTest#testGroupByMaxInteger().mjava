    public void testGroupByMaxInteger() {
        GroupByVisitor visitor =
                new GroupByVisitor(
                        Aggregate.MAX,
                        ff.property("id"),
                        Arrays.asList(ff.property("id")),
                        new NullProgressListener());
        assertEquals(
                Arrays.asList(Integer.class),
                getResultTypes(visitor, Integer.class, Integer.class));
    }

