    @Test
    public void testAllDoesNotMatch() throws Exception {
        FilterFactoryImpl ff = new FilterFactoryImpl();
        Function func =
                ff.function(
                        "equalTo",
                        ff.literal(Arrays.asList(1, 1, 3)),
                        ff.literal(Arrays.asList(1)),
                        ff.literal("ALL"));
        assertFalse((Boolean) func.evaluate(new Object()));
    }

