    public void testGroupByAverageDouble() {
        GroupByVisitor visitor =
                new GroupByVisitor(
                        Aggregate.AVERAGE,
                        ff.property("id"),
                        Arrays.asList(ff.property("flow")),
                        new NullProgressListener());
        assertEquals(
                Arrays.asList(Double.class), getResultTypes(visitor, Integer.class, Double.class));
    }

