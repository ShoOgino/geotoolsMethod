    @Override
    public String toString() {
        StringBuilder s = new StringBuilder();
        if (position != null) {
            s.append("position:").append(position);
        }
        if (begunBy != null) {
            if (s.length() > 0) {
                s.append(", ");
            }
            s.append("begunBy:").append(begunBy);
        }
        if (endBy != null) {
            if (s.length() > 0) {
                s.append(", ");
            }
            s.append("endBy:").append(endBy);
        }
        return s.insert(0, "Instant{").append('}').toString();
    }

