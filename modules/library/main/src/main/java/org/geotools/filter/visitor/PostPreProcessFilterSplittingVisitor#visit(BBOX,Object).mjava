        public Object visit(BBOX filter, Object notUsed) {
        	if (filter instanceof BBOX3D && !fcs.supports(BBOX3D.class)) {
                postStack.push(filter);
        	} else if (!fcs.supports(BBOX.class)) {
	            postStack.push(filter);
	        } else {
	            preStack.push(filter);
	        }
	            
            return null;
        }

