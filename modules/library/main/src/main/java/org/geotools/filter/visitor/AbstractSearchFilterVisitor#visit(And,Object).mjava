    @Override
    public Object visit(And filter, Object data) {
        if (found(data)) {
            return data; // short cut
        }
        if (filter.getChildren() != null) {
            for (Filter child : filter.getChildren()) {
                data = child.accept(this, data);
                if (found(data)) {
                    return data;
                }
            }
        }
        return data;
    }

