    public List<AttributeType> getTypes() {
        if (types == null) {
            synchronized (this) {
                if (types == null) {
                    ArrayList<AttributeType> temp = new ArrayList<>();
                    for (AttributeDescriptor ad : getAttributeDescriptors()) {
                        temp.add(ad.getType());
                    }
                    types = temp;
                }
            }
        }
        return types;
    }

