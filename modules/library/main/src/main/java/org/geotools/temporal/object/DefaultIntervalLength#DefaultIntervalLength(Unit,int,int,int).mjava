    /**
     * Creates a new instance of IntervalUnit example : Unit="second" radix=10 factor=3 value=7
     * specifies a time interval length of 7ms.
     */
    public DefaultIntervalLength(Unit unit, int radix, int factor, int value) {
        this.unit = unit;
        this.radix = radix;
        this.factor = factor;
        this.value = value;
    }

