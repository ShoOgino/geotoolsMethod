    /**
     * Helper method to pull out strings from featureId set.
     * 
     * @return
     */
    private Set fids() {
        Set set = new TreeSet();
        for (Iterator i = fids.iterator(); i.hasNext();) {
            Identifier id = (Identifier) i.next();
            set.add(id.getID().toString());
        }

        return set;
    }

