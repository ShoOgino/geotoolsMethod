    @Override
    public <T> Parameter<T> parameter(
            String name,
            Class<T> type,
            InternationalString title,
            InternationalString description,
            boolean required,
            int minOccurs,
            int maxOccurs,
            T defaultValue) {
        return new org.geotools.data.Parameter<>(
                name, type, title, description, required, minOccurs, maxOccurs, defaultValue, null);
    }
