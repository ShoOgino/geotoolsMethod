    /** Sets this envelope to the specified bounding box. */
    @Override
    public void init(BoundingBox bounds) {
        init(
                bounds.getMinimum(0),
                bounds.getMaximum(0),
                bounds.getMinimum(1),
                bounds.getMaximum(1),
                bounds.getMinimum(2),
                bounds.getMaximum(2));
        this.crs = bounds.getCoordinateReferenceSystem();
    }

