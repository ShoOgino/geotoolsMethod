    public BBOX bbox(
            Expression e,
            double minx,
            double miny,
            double maxx,
            double maxy,
            String srs,
            MatchAction matchAction) {

        BBOXImpl box = bbox2d(e, new ReferencedEnvelope(minx, maxx, miny, maxy, null), matchAction);
        box.setSRS(srs);

        return box;
    }

