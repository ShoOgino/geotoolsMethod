    protected TemporalPrimitive toTemporal(Object object, Expression e) {
        TemporalPrimitive p = toPeriod(object, e);
        if (p != null) {
            return p;
        }
        
        p = toInstant(object, e);
        if (p != null) {
            return p;
        }
        
        return null;
    }

