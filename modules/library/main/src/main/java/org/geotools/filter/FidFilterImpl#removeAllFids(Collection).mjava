    /**
     * Removes a collection of feature IDs from the filter.
     *
     * @param fidsToRemove A collection of feature IDs.
     */
    public void removeAllFids(Collection fidsToRemove) {
        if (fidsToRemove == null) return;

        for (Object o : fidsToRemove) {
            String fid = (String) o;
            removeFid(fid);
        }
    }

