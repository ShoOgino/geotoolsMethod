    /** Frees all State held by this Transaction. */
    @Override
    public synchronized void close() {
        for (Object o : stateLookup.values()) {
            State state = (State) o;
            state.setTransaction(null);
        }
        stateLookup.clear();
        stateLookup = null;
        authorizations.clear();
        authorizations = null;
        propertyLookup.clear();
        propertyLookup = null;
    }

