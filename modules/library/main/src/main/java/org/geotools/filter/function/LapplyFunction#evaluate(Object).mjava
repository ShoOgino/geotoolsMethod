    @Override
    public Object evaluate(Object feature) {
        List<?> source = getExpression(0).evaluate(feature, List.class);
        if (source == null) {
            return null;
        }

        List<Object> result = new ArrayList<>();
        for (Object item : source) {
            result.add(getExpression(1).evaluate(item));
        }

        return result;
    }

