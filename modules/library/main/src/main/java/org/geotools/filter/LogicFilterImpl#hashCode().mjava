    /**
     * Override of hashCode method.
     *
     * @return a code to hash this object by.
     */
    public int hashCode() {
        if(cachedHash == 0) {
            int result = 17;
            int filterType = Filters.getFilterType(this);
            result = (37 * result) + filterType;
            result = (37 * result) + children.hashCode();
            cachedHash = result;
        }

        return cachedHash;
    }

