    @Override
    public void visit(ChannelSelection cs) {
        if (cs.getGrayChannel() != null) {
            cs.getGrayChannel().accept(this);
        }
        if (cs.getRGBChannels() != null) {
            for (SelectedChannelType ch : cs.getRGBChannels()) {
                if (ch != null) {
                    ch.accept(this);
                }
            }
        }
    }

