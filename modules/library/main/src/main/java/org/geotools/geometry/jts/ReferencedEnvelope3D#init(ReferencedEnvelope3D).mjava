    /**
     * Initialize an <code>Envelope</code> from an existing 3D Envelope.
     *
     * @param env the 3D Envelope to initialize from
     */
    public void init(ReferencedEnvelope3D env) {
        super.init((Envelope) env);
        this.minz = env.minz;
        this.maxz = env.maxz;
    }

