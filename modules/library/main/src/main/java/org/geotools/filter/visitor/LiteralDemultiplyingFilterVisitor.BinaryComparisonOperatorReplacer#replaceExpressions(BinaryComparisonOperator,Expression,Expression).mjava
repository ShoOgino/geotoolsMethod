        @Override
        public Filter replaceExpressions(
                BinaryComparisonOperator filter, Expression expression1, Expression expression2) {
            try {
                return (Filter)
                        method.invoke(
                                ff,
                                expression1,
                                expression2,
                                filter.isMatchingCase(),
                                filter.getMatchAction());
            } catch (Exception e) {
                throw new RuntimeException(e);
            }
        }

