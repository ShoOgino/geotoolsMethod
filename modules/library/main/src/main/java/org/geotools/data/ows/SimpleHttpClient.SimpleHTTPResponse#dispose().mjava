        /**
         * @see org.geotools.data.ows.HTTPResponse#dispose()
         */
        public void dispose() {
            if (responseStream != null) {
                try {
                    responseStream.close();
                } catch (IOException e) {
                    // ignore
                }
                responseStream = null;
            }
            if (connection != null) {
                connection.disconnect();
                connection = null;
            }
        }

