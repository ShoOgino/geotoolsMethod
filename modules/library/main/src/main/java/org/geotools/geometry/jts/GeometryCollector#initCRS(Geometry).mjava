    private void initCRS(Geometry g) {
        // see if we have a native CRS in the mix
        if(crs == null && g.getUserData() instanceof CoordinateReferenceSystem) {
            crs = (CoordinateReferenceSystem) g.getUserData();
        }
        if(srid == -1 && g.getSRID() > 0) {
            srid = g.getSRID();
        }
    }

