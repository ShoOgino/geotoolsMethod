    @Override
    public boolean evaluateInternal(Geometry left, Geometry right) {

        switch (literals) {
            case BOTH:
                return cacheValue;
            case RIGHT:
                {
                    return rightPreppedGeom.disjoint(left);
                }
            case LEFT:
                {
                    return leftPreppedGeom.disjoint(right);
                }
            default:
                {
                    return basicEvaluate(left, right);
                }
        }
    }

