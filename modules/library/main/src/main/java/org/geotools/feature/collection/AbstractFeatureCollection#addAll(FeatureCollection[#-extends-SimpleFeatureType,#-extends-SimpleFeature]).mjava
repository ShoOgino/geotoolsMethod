    public boolean addAll(FeatureCollection<? extends SimpleFeatureType,? extends SimpleFeature> c) {
        boolean modified = false;
        FeatureIterator<? extends SimpleFeature> e = c.features();
        try {
            while (e.hasNext()) {
                if (add(e.next()))
                modified = true;
            }
        }
        finally {
            e.close();
        }
        return modified;
    }

