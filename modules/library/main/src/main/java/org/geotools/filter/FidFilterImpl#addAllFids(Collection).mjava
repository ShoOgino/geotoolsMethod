    /**
     * Adds a collection of feature IDs to the filter.
     * 
     * @param fidsToAdd
     *            A collection of feature IDs as strings.
     */
    public void addAllFids(Collection fidsToAdd) {
        if (fidsToAdd == null)
            return;

        for (Iterator i = fidsToAdd.iterator(); i.hasNext();) {
            String fid = (String) i.next();
            addFid(fid);
        }
    }

