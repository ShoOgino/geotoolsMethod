    /**
     * Adds a collection of feature IDs to the filter.
     *
     * @param fidsToAdd A collection of feature IDs as strings.
     */
    public void addAllFids(Collection fidsToAdd) {
        if (fidsToAdd == null) return;

        for (Object o : fidsToAdd) {
            String fid = (String) o;
            addFid(fid);
        }
    }

