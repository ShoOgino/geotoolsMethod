    /** Support method to implement {@link #compareAscending(SimpleFeature, SimpleFeature)} */
    protected int compareAscending(Comparable o1, Comparable o2) {
        if (o1 == null) {
            if (o2 == null) {
                return 0;
            } else {
                return -1;
            }
        } else if (o2 == null) {
            return 1;
        } else {
            return o1.compareTo(o2);
        }
    }

