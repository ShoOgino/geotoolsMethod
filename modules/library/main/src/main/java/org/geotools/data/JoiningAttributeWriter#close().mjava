    /* (non-Javadoc)
     * @see org.geotools.data.AttributeWriter#close()
     */
    @Override
    public void close() throws IOException {
        IOException dse = null;
        for (AttributeWriter writer : writers) {
            try {
                writer.close();
            } catch (IOException e) {
                dse = e;
            }
        }
        if (dse != null) throw dse;
    }

