    static boolean equals(List attributesA, List attributesB, boolean compareUserMaps) {
        return equals(
                (AttributeDescriptor[])
                        attributesA.toArray(new AttributeDescriptor[attributesA.size()]),
                (AttributeDescriptor[])
                        attributesB.toArray(new AttributeDescriptor[attributesB.size()]),
                compareUserMaps);
    }

