    @Override
    public String toString() {
        StringBuilder s = new StringBuilder();
        if (begining != null) {
            s.append("begin:").append(begining);
        }
        if (ending != null) {
            if (s.length() > 0) {
                s.append(", ");
            }
            s.append("end:").append(ending);
        }
        return s.insert(0, "Period{").append('}').toString();
    }

