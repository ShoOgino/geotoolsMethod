    private static boolean matchClientHint(Class<?> cls, Object val) {
        if (val == null) {
            return true;
        }

        if (val instanceof String) {
            final String clsName = (String) val;
            return cls.getName().equals(clsName);
        } else {
            final Class<?> cls2 = (Class<?>) val;
            return cls2 == cls;
        }
    }

