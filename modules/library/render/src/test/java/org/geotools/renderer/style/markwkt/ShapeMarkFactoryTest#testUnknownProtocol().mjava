    @Test
    public void testUnknownProtocol() {
        MeteoMarkFactory smf = new MeteoMarkFactory();
        try {
            this.exp = ff.literal("xxx://triangle");
            if (smf.getShape(null, this.exp, this.feature) == null) {
                Assert.assertTrue(true);
                return;
            }
        } catch (Exception e) {
            Assert.fail();
            return;
        }

        Assert.fail();
    }

