    public void testSouthArrow() {
        MeteoMarkFactory smf = new MeteoMarkFactory();
        try {
            this.exp = ff.literal("extshape://sarrow");
            Shape shape = smf.getShape(null, this.exp, this.feature);
            // Check if the South Arrow is present
            assertNotNull(shape);
        } catch (Exception e) {
            assertTrue(false);
            return;
        }

        assertTrue(true);
    }

