	public static int lookUpCap(String capType) {
		if (SLDStyleFactory.CAP_LOOKUP.containsKey(capType)) {
			return ((Integer) CAP_LOOKUP.get(capType)).intValue();
		} else {
			return java.awt.BasicStroke.CAP_SQUARE;
		}
	}

