    private <T> List<T> evalToList(Expression exp, Object f, List<T> fallback) {
        if (exp == null) {
            return fallback;
        }
        @SuppressWarnings("unchecked")
        List<T> l = exp.evaluate(f, List.class);
        if (l != null) {
            return l;
        }
        return fallback;
    }

