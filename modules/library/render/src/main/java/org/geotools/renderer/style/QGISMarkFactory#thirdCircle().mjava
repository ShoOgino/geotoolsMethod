    private static Shape thirdCircle() {
        // Top-left third
        Arc2D.Double d = new Arc2D.Double(new Rectangle2D.Double(-.5, -.5, 1., 1.), 150., 120.,
                Arc2D.PIE);

        ExplicitBoundsShape shape = new ExplicitBoundsShape(d);
        shape.setBounds(new Rectangle2D.Double(-.5, .5, 1., 1.));

        return shape;
    }

