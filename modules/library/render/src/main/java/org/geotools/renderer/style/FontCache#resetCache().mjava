    /**
     * Resets the font loading cache. If any font was manually registered, it will have to be
     * registered again
     */
    public synchronized void resetCache() {
        if (systemFonts != null) {
            systemFonts.clear();
        }
        if (loadedFonts != null) {
            loadedFonts.clear();
        }
        if (alternatives != null) {
            alternatives.clear();
        }
    }

