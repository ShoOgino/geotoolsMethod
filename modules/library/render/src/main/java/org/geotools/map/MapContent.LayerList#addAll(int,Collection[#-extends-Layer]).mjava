        @Override
        public boolean addAll(int index, Collection<? extends Layer> c) {
            boolean added = super.addAll(index, c);
            if (layerListener != null) {
                for (Layer element : c) {
                    element.addMapLayerListener(layerListener);
                }
            }
            fireLayerAdded(null, index, size() - 1);
            return added;
        }

