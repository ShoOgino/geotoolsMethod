        @Override
        public void lastPositionResults(boolean conflict) {
            if(!conflict) {
                // move on to the next location
                symbols++;
                moveToNextCell();
            } else {
                // can we retry?
                if(attempts > MAX_RANDOM_ATTEMPTS_MULTIPLIER) {
                    // too many attempts, this cell will be left empty
                    moveToNextCell();
                }
            }
        }

