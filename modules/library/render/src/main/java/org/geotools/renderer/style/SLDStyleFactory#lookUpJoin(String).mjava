    public static int lookUpJoin(String joinType) {
        if (SLDStyleFactory.JOIN_LOOKUP.containsKey(joinType)) {
            return ((Integer) JOIN_LOOKUP.get(joinType)).intValue();
        } else {
            return java.awt.BasicStroke.JOIN_MITER;
        }
