    @Override
    public void dispose() {
        preDispose();
        if (reader != null) {
            try {
                reader.dispose();
            } catch (Exception e) {
                // eat me
            }
            reader = null;
        }
        if (style != null) {
            this.style = null;
        }
        if (params != null) {
            this.params = null;
        }
        super.dispose();
    }

