    public NADConGridShift(
            double xmin,
            double ymin,
            double xmax,
            double ymax,
            double dx,
            double dy,
            int width,
            int height) {
        super(width, height);
        this.minX = xmin;
        this.maxX = xmax;
        this.minY = ymin;
        this.maxY = ymax;
        this.dx = dx;
        this.dy = dy;
    }

