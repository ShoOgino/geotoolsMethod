    @Override
    public boolean equals(Object object) {
        if (object == this) {
            // Slight optimization
            return true;
        }

        if (super.equals(object)) {
            final NADCONTransform that = (NADCONTransform) object;

            return this.grid.equals(that.grid);
        } else {
            return false;
        }
    }

