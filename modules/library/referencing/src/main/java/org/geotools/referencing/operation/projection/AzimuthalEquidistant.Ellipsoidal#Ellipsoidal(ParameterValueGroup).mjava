        /**
         * Constructor.
         *
         * @param parameters the parameters that define this projection
         * @throws ParameterNotFoundException
         */
        protected Ellipsoidal(ParameterValueGroup parameters) throws ParameterNotFoundException {
            super(parameters);
            switch (mode) {
                case NORTH_POLAR:
                    Mp = mlfn(HALF_PI, 1, 0);
                    geodesic = null;
                    break;
                case SOUTH_POLAR:
                    Mp = mlfn(-HALF_PI, -1, 0);
                    geodesic = null;
                    break;
                case EQUATORIAL:
                case OBLIQUE:
                    Mp = Double.NaN;
                    geodesic = buildGeodesic();
                    break;
                default:
                    throw new RuntimeException(
                            "Unexpected mode "
                                    + mode
                                    + " for ellipsoidal AzimuthalEquidistant projection");
            }
        }

