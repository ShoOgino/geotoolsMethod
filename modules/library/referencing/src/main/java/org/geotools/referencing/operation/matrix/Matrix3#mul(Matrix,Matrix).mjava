    /**
     * In-place update from matrix1 * matrix2.
     * @param matrix1
     * @param matrix2
     */
    public void mul(Matrix matrix1, Matrix matrix2) {
        DMatrix3x3 a = internal(matrix1);
        DMatrix3x3 b = internal(matrix2);
        if( a == mat || b == mat ){
            mat = new DMatrix3x3();
        }
        CommonOps_DDF3.mult(a, b, mat);
    }

