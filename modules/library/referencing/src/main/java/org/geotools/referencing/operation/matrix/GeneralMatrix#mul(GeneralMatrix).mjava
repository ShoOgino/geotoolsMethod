    // In-place operations
    //
    /**
     * In-place multiply with provided matrix.
     * @param matrix
     * 
     */
    public final void mul(GeneralMatrix matrix){
        DenseMatrix64F ret = new DenseMatrix64F(mat.numRows,matrix.mat.numCols);
        CommonOps.mult(mat,matrix.mat,ret);
        mat = ret;
    }

