    /** Compares the specified object with this map projection for equality. */
    @Override
    public boolean equals(final Object object) {
        if (object == this) {
            // Slight optimization
            return true;
        }
        if (super.equals(object)) {
            final Polyconic that = (Polyconic) object;
            return equals(this.ml0, that.ml0);
        }
        return false;
    }

