    /** Returns the suffix for the specified field. */
    private String getSuffix(final int index) {
        switch (index) {
            case PREFIX_FIELD:
                return prefix;
            case DEGREES_FIELD:
                return suffix0;
            case MINUTES_FIELD:
                return suffix1;
            case SECONDS_FIELD:
                return suffix2;
            default:
                return null;
        }
    }

