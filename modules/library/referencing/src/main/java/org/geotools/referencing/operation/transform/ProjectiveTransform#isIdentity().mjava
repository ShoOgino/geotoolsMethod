    /** Tests whether this transform does not move any points. */
    @Override
    public boolean isIdentity() {
        if (numRow != numCol) {
            return false;
        }
        int index = 0;
        for (int j = 0; j < numRow; j++) {
            for (int i = 0; i < numCol; i++) {
                if (elt[index++] != (i == j ? 1 : 0)) {
                    return false;
                }
            }
        }
        assert isIdentity(0);
        return true;
    }

