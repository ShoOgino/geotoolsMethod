    /**
     * Sets this {@code Rectangle2D} to be the same as the specified {@code Rectangle2D}.
     *
     * @param r the specified {@code Rectangle2D}
     */
    @Override
    public void setRect(final Rectangle2D r) {
        this.xmin = r.getMinX();
        this.ymin = r.getMinY();
        this.xmax = r.getMaxX();
        this.ymax = r.getMaxY();
    }

