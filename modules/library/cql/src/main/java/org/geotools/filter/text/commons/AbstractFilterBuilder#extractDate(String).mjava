    /**
     * Extracts the Date from cql date time
     *
     * @param cqlDateTime
     * @return String with the date
     */
    private String extractDate(final String cqlDateTime) {
        Matcher m = DATETIME_PATTERN.matcher(cqlDateTime);
        if (m.matches()) {
            assert m.group(1) != null;
            return m.group(1);
        }
        assert false;
        return null;
    }

