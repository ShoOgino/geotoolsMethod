    /**
     * Builds a bbox using the stack subproducts
     *
     * @return {@link BBOX}}
     * @throws CQLException
     */
    public BBOX buildBBoxWithCRS() throws CQLException {

        String crs = getResultStack().popStringValue();
        assert crs != null;
        BBOX bbox = buildBBox(crs);
        return bbox;
    }

