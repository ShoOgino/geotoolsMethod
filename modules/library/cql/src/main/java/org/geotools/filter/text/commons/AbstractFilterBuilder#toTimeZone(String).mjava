    /**
     * Remove the ":". The Z format is [sign]hhmm 
     * @param cqlTimeZone  [sign]hh:mm
     * @return [sign]hhmm
     */
    private String toTimeZone(final String cqlTimeZone) {
		
    	String[] str = cqlTimeZone.split(":");
    	assert str.length == 2;
    	return  str[0].concat(str[1]);
	}

