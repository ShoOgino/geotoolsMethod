    /**
     * Extracts the time
     *
     * @param cqlDateTime
     * @return the time or a null string
     */
    private String extractTime(String cqlDateTime) {
        Matcher m = DATETIME_PATTERN.matcher(cqlDateTime);
        if (m.matches()) {
            assert m.group(2) != null;
            return m.group(2);
        }

        assert false;
        return null;
    }

