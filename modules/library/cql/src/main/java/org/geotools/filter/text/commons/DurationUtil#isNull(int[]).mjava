    /**
     * durDate is null if all his values are -1
     *
     * @param durDate
     * @return true if has some greater than or equal 0
     */
    private static boolean isNull(int[] durDate) {
        for (int i = 0; i < durDate.length; i++) {
            if (durDate[i] >= 0) {
                return false;
            }
        }

        return true;
    }

