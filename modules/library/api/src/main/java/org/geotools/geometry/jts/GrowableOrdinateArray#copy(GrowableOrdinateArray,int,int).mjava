    /**
     * Copies a sub-array from another growable array
     *
     * @param other
     * @param from
     * @param to
     */
    public void copy(GrowableOrdinateArray other, int from, int to) {
        ensureLength(to + 1);
        System.arraycopy(other.data, 0, data, 0, to + 1);
        this.curr = to + 1;
    }

