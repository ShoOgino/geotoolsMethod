	/**
	 * Releases resources held by this {@link AbstractGridCoverageWriter}.
	 */
	public void dispose() {
		if(LOGGER.isLoggable(Level.FINE))
			LOGGER.fine("Disposing writer:"+destination);
		
		if (outStream != null) {
			try {
				outStream.flush();			

			} catch (IOException e) {

			}
			finally{
				try {
					outStream.close();
				} catch (Throwable e) {

				}
			}
			
			
		}

	}

