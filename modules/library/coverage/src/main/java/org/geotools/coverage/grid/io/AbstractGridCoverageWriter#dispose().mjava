    /** Releases resources held by this {@link AbstractGridCoverageWriter}. */
    @Override
    public void dispose() {
        if (LOGGER.isLoggable(Level.FINE)) LOGGER.fine("Disposing writer:" + destination);

        if (outStream != null) {
            try {
                outStream.flush();

            } catch (IOException e) {

            } finally {
                try {
                    outStream.close();
                } catch (Throwable e) {

                }
                outStream = null;
                destination = null;
            }
        }
    }

