    /**
     * DiffFeatureWriter construction.
     * 
     * @param reader
     * @param diff
     * @param filter
     */
    public DiffContentFeatureWriter(ContentFeatureStore store, Diff diff,
            FeatureReader<SimpleFeatureType, SimpleFeature> reader) {
        this.store = store;
        this.reader = reader;
        this.state = store.getState();
        this.diff = diff;
    }

