    /**
     * @see org.geotools.data.jdbc.fidmapper.FIDMapper#getColumnDecimalDigits(int)
     */
    public int getColumnDecimalDigits(int colIndex) {
        if (colIndex > 0 && colIndex < getColumnCount()) {
            return colDecimalDigits[colIndex];
        } else {
            return 0;
        }
    }

