    /**
     * 
     * @see org.geotools.data.AttributeWriter#hasNext()
     */
    public boolean hasNext() throws IOException {
        try {
            if (!hasNextCalled) {
                hasNextCalled = true;
                lastNext = resultSet.next();
            }
        } catch (Exception e) {
            throw new DataSourceException("Problem moving on to the next attribute", e);
        }

        return lastNext;
    }

