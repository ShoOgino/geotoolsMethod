    /**
     * Returns the geometry native srid, or -1 if not known
     * 
     * @param geometryName
     * @return
     */
    public int getNativeSrid(String geometryName) {
        Integer srid = nativeSrids.get(geometryName);
        if (srid == null) {
            srid = -1;
        }
        return srid;
    }

