    /**
     * Extracts the set of params available for a given factory
     *
     * @param factory
     * @return
     */
    protected List<String> getParamKeys(JDBCDataStoreFactory factory) {
        Param[] params = factory.getParametersInfo();
        List<String> results = new ArrayList<String>();
        for (Param p : params) {
            results.add(p.key);
        }

        return results;
    }

