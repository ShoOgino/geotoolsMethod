     *
     * <pre>{@code
     * INSERT INTO lines_m (id, description, geometry) VALUES
     * (1, 'line_m_a', ST_SetSRID(ST_GeomFromEWKT('LINESTRINGM(-2 1 0, 3 1 1, 3 5 2, -2 5 3)'), 4326));
     * }</pre>
     */
    protected abstract void createTableLinesM() throws Exception;

