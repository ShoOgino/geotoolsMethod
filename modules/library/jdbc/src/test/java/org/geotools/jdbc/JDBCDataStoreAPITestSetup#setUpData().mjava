    protected void setUpData() throws Exception {
        // kill all the data
        try {
            dropRoadTable();
        } catch (SQLException e) {
        }

        try {
            dropRiverTable();
        } catch (SQLException e) {
        }

        try {
            dropLakeTable();
        } catch (SQLException e) {
        }

        try {
            dropBuildingTable();
        } catch (SQLException e) {
        }

        // create all the data
        createRoadTable();
        createRiverTable();
        createLakeTable();
    }

