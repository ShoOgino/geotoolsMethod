    public void testSimpleIn() throws FilterToSQLException {
        // straight
        assertEquals(encodeInComparison("in", true, "true", 1, 2), "WHERE testAttr IN (1, 2)");
        // double negation
        assertEquals(encodeInComparison("in", false, "false", 1, 2), "WHERE testAttr IN (1, 2)");
    }

