    @Test
    public void testSimpleIn() throws FilterToSQLException {
        // straight
        Assert.assertEquals(
                encodeInComparison("in", true, "true", 1, 2), "WHERE testAttr IN (1, " + "2)");
        // double negation
        Assert.assertEquals(
                encodeInComparison("in", false, "false", 1, 2), "WHERE testAttr IN " + "(1, 2)");
    }

