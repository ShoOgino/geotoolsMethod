    public void testEscapeName() {
        encoder.setSqlNameEscape("\"");
        assertEquals("\"abc\"", encoder.escapeName("abc"));
        assertEquals("\"\"\"abc\"", encoder.escapeName("\"abc"));
        assertEquals("\"a\"\"bc\"", encoder.escapeName("a\"bc"));
        assertEquals("\"abc\"\"\"", encoder.escapeName("abc\""));
        encoder.setSqlNameEscape("");
        assertEquals("abc", encoder.escapeName("abc"));
    }

