    /** */
    public void rollback() throws IOException {
        failIfClosed();
        try {
            versionHandler.rollbackEditState();
            // fire changes in the calling thread
            fireChanges(false);
        } catch (IOException se) {
            // release resources
            close();
            LOGGER.log(Level.WARNING, se.getMessage(), se);
            throw se;
        }
    }

