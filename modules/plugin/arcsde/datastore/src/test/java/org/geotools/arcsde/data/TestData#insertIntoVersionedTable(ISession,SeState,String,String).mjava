    public void insertIntoVersionedTable(
            final ISession session,
            final SeState state,
            final String tableName,
            final String nameField)
            throws IOException {

        session.issue(
                new Command<Void>() {

                    @Override
                    public Void execute(ISession session, SeConnection connection)
                            throws SeException, IOException {
                        final SeInsert insert = new SeInsert(connection);
                        SeObjectId differencesId = new SeObjectId(SeState.SE_NULL_STATE_ID);
                        insert.setState(
                                state.getId(), differencesId, SeState.SE_STATE_DIFF_NOCHECK);

                        insert.intoTable(tableName, new String[] {"NAME"});
                        SeRow row = insert.getRowToSet();
                        row.setString(0, "NAME 1");
                        insert.execute();
                        insert.close();
                        return null;
                    }
                });
    }

