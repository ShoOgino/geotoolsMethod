        final int reqWidth = 1000; // originalGridRange.getSpan(0) / 30;
        final int reqHeight = 447; // originalGridRange.getSpan(1) / 30;

        requestEnvelope.setEnvelope(
                249403.30079879, 929431.93501181, 251110.35576095, 930194.98857989);
