    @Override
    @SuppressWarnings("PMD.CloseResource") // actually closes everything
    public void close() throws IOException {
        try {
            try {
                rs.close();
            } finally {
                try {
                    st.close();
                } finally {
                    cx.close();
                }
            }
        } catch (SQLException e) {
            throw new IOException(e);
        }
    }

