    public void addRange(String attribute, Integer low, Integer high, List<String> q) {
        if (low != null && high != null && low == high) {
            q.add(attribute + " = " + low);
        } else {
            if (low != null) {
                q.add(attribute + " >= " + low);
            }
            if (high != null) {
                q.add(attribute + " <= " + high);
            }
        }
    }

