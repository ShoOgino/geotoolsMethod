    private static CoordinateReferenceSystem decodeCRSFromResultset(ResultSet rs, int srid)
            throws IOException {
        try {
            return decodeSRID(srid);
        } catch (Exception ex) {
            // try parsing srtext directly
            try {
                return CRS.parseWKT(rs.getString("srtext"));
            } catch (Exception e2) {
                throw new IOException(ex);
            }
        }
    }

