    public GeoPkgMetadataReference(
            Scope scope,
            String table,
            String column,
            Long rowId,
            Date timestamp,
            GeoPkgMetadata metadata,
            GeoPkgMetadata metadataParent) {
        this.scope = scope;
        this.table = table;
        this.column = column;
        this.rowId = rowId;
        this.timestamp = timestamp;
        this.metadata = metadata;
        this.metadataParent = metadataParent;
    }

