    @Test
    public void testParseNestedObject() {
        assertThrows(
                java.lang.IllegalArgumentException.class,
                new ThrowingRunnable() {

                    @Override
                    public void run() throws Throwable {
                        String[] parsed =
                                io.parse(
                                        "[\"a\",\"b\", { \"key\" = \"value\"}]",
                                        String.class,
                                        null);
                    }
                });
    }

