    public Object[] toArray() {
        Object[] res = backendCollection.toArray();
        for (int i = 0; i < res.length; i++) {
            res[i] =
                    new PreGeneralizedSimpleFeature(
                            getSchema(),
                            getSchema(),
                            indexMapping,
                            (SimpleFeature) res[i],
                            geomPropertyName,
                            backendGeomPropertyName);
        }
        return res;
    }

