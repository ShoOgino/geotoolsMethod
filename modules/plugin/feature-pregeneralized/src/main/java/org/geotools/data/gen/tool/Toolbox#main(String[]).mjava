    public static void main(String[] args) {
        Toolbox toolBox = new Toolbox();

        boolean retval = false;
        try {
            retval = toolBox.parse(args);
        } catch (IOException e) {
            java.util.logging.Logger.getGlobal().log(java.util.logging.Level.INFO, "", e);
            retval = false;
        }

        if (retval) System.exit(0);
        else System.exit(1);
    }

