    @Override
    protected void disposeTileIndexStore() {
        try {
            if (tileIndexStore != null) {
                tileIndexStore.dispose();
            }
        } catch (Throwable e) {
            if (LOGGER.isLoggable(Level.FINE)) {
                LOGGER.log(Level.FINE, e.getLocalizedMessage(), e);
            }
        } finally {
            this.tileIndexStore = null;
        }
    }

