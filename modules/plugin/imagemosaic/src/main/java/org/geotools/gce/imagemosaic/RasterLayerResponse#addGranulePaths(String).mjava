    public void addGranulePaths(String granulesPaths) {
        if (granulesPaths == null) return;

        if (this.granulesPaths == null || this.granulesPaths.isEmpty()) {
            this.granulesPaths = granulesPaths;
        } else if (!granulesPaths.isEmpty()) {
            this.granulesPaths += "," + granulesPaths;
        }
    }

