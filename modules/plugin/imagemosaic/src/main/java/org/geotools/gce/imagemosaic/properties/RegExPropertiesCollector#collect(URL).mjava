    @Override
    public RegExPropertiesCollector collect(URL url) {
        super.collect(url);

        // get name of the url
        final String fullUrl = url.toString();
        String name = fullUrl;
        if (!fullPath) {
            int index = name.lastIndexOf("/");
            name = name.substring(index + 1);
        }

        // get matches
        addMatches(name);
        return this;
    }

