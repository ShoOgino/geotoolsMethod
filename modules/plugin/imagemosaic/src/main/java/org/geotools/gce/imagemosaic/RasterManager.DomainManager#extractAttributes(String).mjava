        private String extractAttributes(String propertyName) {
            if (propertyName.contains("(") && propertyName.contains(")")) {
                // extract the ranges attributes
                propertyName =
                        propertyName
                                .substring(propertyName.indexOf("("))
                                .replace("(", "")
                                .replace(")", "");
            }
            return propertyName;
        }

