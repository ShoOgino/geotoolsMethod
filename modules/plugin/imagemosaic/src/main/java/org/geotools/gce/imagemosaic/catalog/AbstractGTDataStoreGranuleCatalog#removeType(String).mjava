    public void removeType(String typeName) throws IOException {
        Utilities.ensureNonNull("featureType", typeName);
        final Lock lock = rwLock.writeLock();
        try {
            lock.lock();
            checkStore();

            getTileIndexStore().removeSchema(typeName);
            removeTypeName(typeName);

        } finally {
            lock.unlock();
        }

    }

