    @Override
    public void dispose() {
        try {
            if (multiScaleROIProvider != null) {
                multiScaleROIProvider.dispose();
            }
        } catch (Throwable e) {
            if (LOGGER.isLoggable(Level.FINE)) {
                LOGGER.log(Level.FINE, e.getLocalizedMessage(), e);
            }
        } finally {
            multiScaleROIProvider = null;
            disposeTileIndexStore();
        }
    }

