    @Test
    public void testCleanUpWkbFootprints() throws Exception {
        assertFootprintsCleanup(
                "footprint_wkbs",
                WKBLoaderSPI.class.getName(),
                true,
                "_%d",
                f -> f.getName().startsWith("r1c1"),
                FF.like(FF.property("location"), "r1c1*"));
    }

