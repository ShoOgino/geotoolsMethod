    @Test
    public void testCleanUpWktFootprints() throws Exception {
        assertFootprintsCleanup(
                "footprint_wkts",
                WKTLoaderSPI.class.getName(),
                true,
                "-%d",
                f -> f.getName().startsWith("r1c1"),
                FF.like(FF.property("location"), "r1c1*"));
    }

