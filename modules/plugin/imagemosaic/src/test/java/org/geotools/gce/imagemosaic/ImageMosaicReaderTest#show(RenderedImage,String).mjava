    /**
     * Shows the provided {@link RenderedImage} ina {@link JFrame} using the provided <code>title
     * </code> as the frame's title.
     *
     * @param image to show.
     * @param title to use.
     */
    static void show(RenderedImage image, String title) {
        ImageIOUtilities.visualize(image, title);
    }

