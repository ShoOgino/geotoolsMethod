    /** Override which checks if the fixture is available. If not the test is not executed. */
    @Override
    public void run(TestResult result) {
        if (gdalAvailable()) {
            super.run(result);
        } else {
            System.out.println(
                    "Skipping test "
                            + getClass().getSimpleName()
                            + " "
                            + getName()
                            + " since GDAL is not available");
        }
    }

