    private static boolean allPointsHave3Coordinates(CoordinateSequence cs) {
        if (cs.getDimension() < 3) {
            return false;
        }
        int size = cs.size();
        for (int i = 0; i < size; ++i) {
            if (Double.isNaN(cs.getOrdinate(i, 2))) {
                return false;
            }
        }
        return true;
    }

