    /**
     * Converts provied list to an int array
     *
     * @param list List to cast to an array type
     * @return array of ints
     */
    private static int[] intArray(List<Integer> list) {
        int[] array = new int[list.size()];
        int offset = 0;
        for (Integer i : list) {
            array[offset++] = i;
        }

        return array;
    }

