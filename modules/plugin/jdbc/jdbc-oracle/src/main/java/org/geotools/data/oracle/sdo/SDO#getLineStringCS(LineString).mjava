    private static CoordinateSequence getLineStringCS(LineString ls) {
        if (ls.getCoordinateSequence() instanceof CoordinateAccess)
        {
            CoordinateAccess ca = (CoordinateAccess) ls.getCoordinateSequence();
            return ca;
        }
        else
            return null;
    }

