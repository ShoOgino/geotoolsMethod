    /**
     * Access STARTING_OFFSET from elemInfo, or -1 if not available.
     *
     * <p>
     */
    private static int STARTING_OFFSET(int[] elemInfo, int triplet) {
        if (((triplet * 3) + 0) >= elemInfo.length) {
            return -1;
        }

        return elemInfo[(triplet * 3) + 0];
    }

