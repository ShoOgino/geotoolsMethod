    @Override
    public Object getLastAutoGeneratedValue(
            String schemaName, String tableName, String columnName, Connection cx, Statement st)
            throws SQLException {
        ResultSet rs = st.getGeneratedKeys();
        try {
            Object result = null;
            if (rs.next()) {
                result = rs.getObject(1);
            }
            return result;
        } finally {
            dataStore.closeSafe(rs);
        }
    }

