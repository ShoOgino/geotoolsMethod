    @Test
    public void testDefaultFetchSizeDataStore() throws Exception {
        JDBCDataStore ds = null;
        try {
            assertNull(params.get(H2DataStoreFactory.FETCHSIZE.key));
            ds = factory.createDataStore(params);
            assertNotNull(ds);
            assertEquals(1000, ds.getFetchSize());
        } finally {
            if (ds != null) {
                ds.dispose();
            }
        }
    }

