    @Test
    public void testDefaultFetchSizeDataStore() throws Exception {
        JDBCDataStore ds = null;
        try {
        	assertNull(params.get(H2DataStoreFactory.FETCHSIZE));
            ds = factory.createDataStore(params);
            assertNotNull(ds);
            assertTrue(ds.getFetchSize() == 1000);
        } finally {
            if (ds != null) {
                ds.dispose();
            }
        }
    }

