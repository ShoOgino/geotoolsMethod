    int query(Filter f) throws Exception {
        SimpleFeatureSource featureSource = dataStore.getFeatureSource("lakesview2");
        Query q = new Query();
        q.setFilter(f);
        int r = 0;
        try (SimpleFeatureIterator features = featureSource.getFeatures(q).features()) {
            while (features.hasNext()) {
                features.next();
                r++;
            }
        }
        return r;
    }

