    @Override
    public void run(TestResult result) {
        // only run if spatialite can load
        try {
            if (new SpatiaLiteDataStoreFactory().isAvailable()) {
                super.run(result);
            }
        } catch (Throwable t) {
            java.util.logging.Logger.getGlobal().log(java.util.logging.Level.INFO, "", t);
        }
    }

