    static void dropSequence(String schemaName, String seqName, Connection con)
            throws SQLException {
        if (existsSequence(schemaName, seqName, con) == false) return;
        PreparedStatement ps =
                con.prepareStatement("drop sequence \"" + schemaName + "\".\"" + seqName + "\"");
        ps.execute();
        ps.close();
    }

