    public static SchemaStoreDirectory getLowestPriority() {
        return directoriies
                .stream()
                .sorted(
                        new Comparator<SchemaStoreDirectory>() {

                            @Override
                            public int compare(SchemaStoreDirectory o1, SchemaStoreDirectory o2) {
                                return o1.getPriority() - o2.getPriority();
                            }
                        })
                .findFirst()
                .get();
    }

