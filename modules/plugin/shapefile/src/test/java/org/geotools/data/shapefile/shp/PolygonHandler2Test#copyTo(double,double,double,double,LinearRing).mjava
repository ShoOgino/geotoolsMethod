    public static LinearRing copyTo(double x, double y, double w, double h, LinearRing g) {
        if (g.getNumPoints() != 5) {
            throw new IllegalArgumentException("Geometry must have 5 points");
        }

        Coordinate[] coords = g.getCoordinates();
        coords[0].x = x;
        coords[0].y = y;
        coords[1].x = x + w;
        coords[1].y = y;
        coords[2].x = x + w;
        coords[2].y = y + h;
        coords[3].x = x;
        coords[3].y = y + h;
        coords[4].x = x;
        coords[4].y = y;

        return g;
    }

