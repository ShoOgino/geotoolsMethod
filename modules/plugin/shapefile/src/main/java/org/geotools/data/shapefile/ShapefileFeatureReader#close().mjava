    @Override
    public void close() throws IOException {
        try {
            if (shp != null) {
                shp.close();
            }
        } finally {
            try {
                if (dbf != null) {
                    dbf.close();
                }
            } finally {
                try {
                    if (fidReader != null) {
                        fidReader.close();
                    }
                } finally {
                    shp = null;
                    dbf = null;
                }
            }
        }
    }

