    @Override
    @SuppressWarnings("deprecation") // finalize is deprecated in Java 9
    protected void finalize() throws Throwable {
        super.finalize();
        if (shpFiles != null && trace != null) {
            LOGGER.log(
                    Level.SEVERE,
                    "Undisposed of shapefile, you should call dispose() on all shapefile stores",
                    trace);
        }
        dispose();
    }

