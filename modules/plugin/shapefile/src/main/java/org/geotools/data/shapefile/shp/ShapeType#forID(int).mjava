    /**
     * Determine the ShapeType for the id.
     *
     * @param id The id to search for.
     * @return The ShapeType for the id.
     */
    public static ShapeType forID(int id) {
        switch (id) {
            case 0:
                return NULL;
            case 1:
                return POINT;
            case 11:
                return POINTZ;
            case 21:
                return POINTM;
            case 3:
                return ARC;
            case 13:
                return ARCZ;
            case 23:
                return ARCM;
            case 5:
                return POLYGON;
            case 15:
                return POLYGONZ;
            case 25:
                return POLYGONM;
            case 8:
                return MULTIPOINT;
            case 18:
                return MULTIPOINTZ;
            case 28:
                return MULTIPOINTM;
            default:
                return UNDEFINED;
        }
    }

