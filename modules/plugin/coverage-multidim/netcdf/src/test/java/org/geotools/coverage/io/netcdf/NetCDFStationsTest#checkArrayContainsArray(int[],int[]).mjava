    /** Checks that arrayA contains arrayB. */
    private void checkArrayContainsArray(int[] arrayA, int[] arrayB) {
        for (int j : arrayB) {
            boolean found = false;
            for (int k : arrayA) {
                if (j == k) {
                    found = true;
                }
            }
            assertThat(found, is(true));
        }
    }

