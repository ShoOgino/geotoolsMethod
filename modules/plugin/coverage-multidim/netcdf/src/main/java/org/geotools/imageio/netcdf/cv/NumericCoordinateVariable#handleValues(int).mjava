    /**
     * @param index
     * @return
     */
    private double handleValues(int index) {
        double val = coordinateAxis.getCoordValue(index);
        if (!Double.isNaN(scaleFactor)) {
            val *= scaleFactor;
        }
        if (!Double.isNaN(offsetFactor)) {
            val += offsetFactor;
        }
        return val;
    }

