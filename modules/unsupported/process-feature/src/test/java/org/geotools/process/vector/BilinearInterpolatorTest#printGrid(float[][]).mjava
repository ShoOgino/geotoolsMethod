    @SuppressWarnings("PMD.ForLoopCanBeForeach")
    private void printGrid(float[][] grid) {
        StringBuilder sb = new StringBuilder();
        for (int j = grid[0].length - 1; j >= 0; j--) {
            for (int i = 0; i < grid.length; i++) {
                sb.append(grid[i][j] + " ");
            }
            LOGGER.info(sb.toString());
        }
    }

