    @Test
    public void testClassifyWithInvalidClassifier() {
        ClassifyByRangeProcess cp = new ClassifyByRangeProcess();
        boolean error = false;
        try {
            SimpleFeatureCollection result =
                    cp.execute(
                            fc,
                            "value",
                            null,
                            new String[] {"A", "B", "C", "D", "E"},
                            "NotExisting",
                            3,
                            true,
                            "class",
                            null);
        } catch (Exception e) {
            error = true;
        }
        assertTrue(error);
    }

