    @Test
    public void testClassifyWrongValues() {
        ClassifyByRangeProcess cp = new ClassifyByRangeProcess();
        boolean error = false;
        try {
            cp.execute(fc, "value", new String[] { "2", "5", "10" },
                    new String[] { "A", "B", "C", "D", "E" }, null, null, true, "class", null);
        } catch (ProcessException e) {
            error = true;
        }
        assertTrue(error);
    }

