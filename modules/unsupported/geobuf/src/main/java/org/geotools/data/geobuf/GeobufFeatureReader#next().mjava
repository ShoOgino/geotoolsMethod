    @Override
    public SimpleFeature next() throws IOException, IllegalArgumentException, NoSuchElementException {
        SimpleFeature feature;
        if (nextFeature != null) {
            feature = nextFeature;
            nextFeature = null;
        } else {
            feature = geobufFeature.decode(data, featureIndex, featureBuilder);
            featureIndex++;
        }
        return feature;
    }

