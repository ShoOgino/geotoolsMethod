    /**
     * Removes all children.
     */
    @Override
    public void removeChildren() {
        final long removed = childrenCost();
        if (removed != 0) {
            SelectedNode parent = this;
            do {
                parent.cost -= removed;
            } while ((parent = (SelectedNode) parent.getParent()) != null);
        }
        super.removeChildren();
    }

