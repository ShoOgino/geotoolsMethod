    /**
     * Returns a hash code value for this overview level.
     */
    @Override
    public int hashCode() {
        int code = ordinal + 37 * (xSubsampling + 37 * (ySubsampling + Arrays.hashCode(patterns)));
        if (finer != null) {
            code += 31 * finer.hashCode();
        }
        return code;
    }

