    @Override
    public boolean equals(Object other) {
        if (other == this) {
            return true;
        }
        if ((other instanceof Error_) == false) {
            return false;
        }
        Error_ rhs = ((Error_) other);
        return new EqualsBuilder()
                .append(code, rhs.code)
                .append(message, rhs.message)
                .append(details, rhs.details)
                .isEquals();
    }

