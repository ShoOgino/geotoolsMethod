    @Override
    public boolean equals(Object other) {
        if (other == this) {
            return true;
        }
        if ((other instanceof Service) == false) {
            return false;
        }
        Service rhs = ((Service) other);
        return new EqualsBuilder()
                .append(name, rhs.name)
                .append(type, rhs.type)
                .append(url, rhs.url)
                .isEquals();
    }

