    @Override
    public boolean equals(Object other) {
        if (other == this) {
            return true;
        }
        if ((other instanceof DrawingInfo) == false) {
            return false;
        }
        DrawingInfo rhs = ((DrawingInfo) other);
        return new EqualsBuilder()
                .append(renderer, rhs.renderer)
                .append(transparency, rhs.transparency)
                .isEquals();
    }

