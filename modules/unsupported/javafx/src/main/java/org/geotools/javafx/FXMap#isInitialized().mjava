    /**
     * Checks, if the map was initialized properly.
     * @return True if map was initialized, else false
     */
    public boolean isInitialized() {
        if (this.wms != null
                && this.mapContent != null
                && this.layers.size() > 0
                && this.getExtent() != null) {
            return true;
        } else {
            return false;
        }
    }

