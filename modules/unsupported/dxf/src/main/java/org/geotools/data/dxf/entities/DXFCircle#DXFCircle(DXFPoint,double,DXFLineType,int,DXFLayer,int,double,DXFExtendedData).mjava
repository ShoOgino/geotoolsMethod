    public DXFCircle(
            DXFPoint p,
            double r,
            DXFLineType lineType,
            int c,
            DXFLayer l,
            int visibility,
            double thickness,
            DXFExtendedData extData) {
        super(c, l, visibility, lineType, thickness);
        _point = p;
        _radius = r;
        setName("DXFCircle");
        _extendedData = extData;
    }

