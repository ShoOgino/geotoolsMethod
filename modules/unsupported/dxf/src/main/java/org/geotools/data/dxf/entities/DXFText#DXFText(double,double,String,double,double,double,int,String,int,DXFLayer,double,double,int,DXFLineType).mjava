    public DXFText(
            double x,
            double y,
            String value,
            double rotation,
            double thickness,
            double height,
            int align,
            String style,
            int c,
            DXFLayer l,
            double angle,
            double zoomFactor,
            int visibility,
            DXFLineType lineType) {
        super(c, l, visibility, lineType, thickness);
        _point = new DXFPoint(x, y, c, l, visibility, thickness);
        _value = value;
        _rotation = rotation;
        _height = height;
        _align = align;
        _style = style;
        _angle = angle;
        _zoomfactor = zoomFactor;
        setName("DXFText");
    }

