    public DXFEllipse(
            DXFPoint centre,
            DXFPoint p,
            double r,
            double s,
            double e,
            int c,
            DXFLayer l,
            int visibility,
            DXFLineType typeLine,
            DXFExtendedData extData) {
        super(c, l, visibility, typeLine, DXFTables.defaultThickness);
        _centre = centre;
        _point = p;
        _ratio = r;
        _end = e;
        _start = s;
        setName("DXFEllipse");
        _extendedData = extData;
    }

