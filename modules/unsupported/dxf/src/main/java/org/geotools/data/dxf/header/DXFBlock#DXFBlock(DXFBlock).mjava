    public DXFBlock(DXFBlock newBlock) {
        this(
                newBlock._point.X(),
                newBlock._point.Y(),
                newBlock._flag,
                newBlock._name,
                null,
                newBlock.getColor(),
                newBlock.getRefLayer());

        // Copy entities
        Iterator iter = newBlock.theEntities.iterator();
        while (iter.hasNext()) {
            theEntities.add(((DXFEntity) iter).clone());
        }
    }

