    public DXFHeader(
            DXFPoint limmin,
            DXFPoint limmax,
            DXFPoint extmin,
            DXFPoint extmax,
            int fillmode,
            String version,
            int srid) {
        _LIMMIN = limmin;
        _LIMMAX = limmax;
        _EXTMIN = extmin;
        _EXTMAX = extmax;
        _FILLMODE = fillmode;
        _ACADVER = version;
        _SRID = srid;
    }

