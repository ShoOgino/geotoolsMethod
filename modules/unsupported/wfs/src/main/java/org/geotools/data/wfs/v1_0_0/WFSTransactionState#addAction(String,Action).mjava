    /**
     * @param a
     */
    public void addAction(String typeName, Action a) {
        synchronized (actionMap) {
            List<Action> list =  actionMap.get(typeName);
            if (list == null) {
                list = new ArrayList<Action>();
                actionMap.put(typeName, list);
            }
            list.add(a);
        }
    }

