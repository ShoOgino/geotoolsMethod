    public Object visit(Not f, Object arg1) {
        // visit child
        f.getFilter().accept(this, arg1);

        // deal with envelope
        if (envelopes.size() > 0) {
            // universe - envelope = universe
            envelopes.pop();
            envelopes.push(new Envelope(UNIVERSE_ENVELOPE));
        }
        otherRestrictions.push(f);
        return null;
    }

