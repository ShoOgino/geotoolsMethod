	/**
	 * Determines if the feature collection contains
	 * all objects in the collection
	 */
	public boolean containsAll(Collection<?> c) {
        Iterator<?> e = c.iterator();
        try {
            while (e.hasNext())
                if(!contains(e.next()))
                return false;
            return true;
        } finally {
            close( (Iterator<SimpleFeature>)e );
        }
	}

