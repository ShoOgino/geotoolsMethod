        @Override
        public void runTest() throws Throwable {
            int count = 0;

            while (count < repeat) {
                Thread.sleep(rand.nextInt(maxdelay));

                if (Thread.interrupted()) {
                    throw new InterruptedException();
                }

                //                System.out.println(Thread.currentThread().getName() + " : cleared cache");
                grid.clear();
                count++;
            }
        }

