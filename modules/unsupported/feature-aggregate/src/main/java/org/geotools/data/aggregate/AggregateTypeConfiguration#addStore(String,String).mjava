    /**
     * Adds a source store/type for this aggregated feature type
     * @param storeName
     * @param typeName
     */
    public void addStore(String localName, String typeName) {
        Name storeName = buildName(localName);
        if (storeMap.isEmpty()) {
            primaryStore = storeName;
        }
        storeMap.put(storeName, typeName);
    }

