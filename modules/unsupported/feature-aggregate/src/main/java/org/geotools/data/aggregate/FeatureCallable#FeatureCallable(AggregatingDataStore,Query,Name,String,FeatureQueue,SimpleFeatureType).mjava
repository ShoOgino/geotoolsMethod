    public FeatureCallable(
            AggregatingDataStore store,
            Query query,
            Name storeName,
            String typeName,
            FeatureQueue queue,
            SimpleFeatureType target) {
        super();
        this.store = store;
        this.query = query;
        this.storeName = storeName;
        this.typeName = typeName;
        this.queue = queue;
        this.builder = new SimpleFeatureBuilder(target);
    }

