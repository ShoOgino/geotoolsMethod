    public List<String> readNextRow() throws IOException {
        String line = reader.readLine();
        if (line == null) {
            return null;
        }

        Scanner scanner = new Scanner(line);
        ArrayList<String> ret = new ArrayList<String>();
        while (true) {
            String entry = scanner.getNextEntry();
            if (entry == null) {
                break;
            }
            ret.add(entry);
        }
        return ret;
    }

