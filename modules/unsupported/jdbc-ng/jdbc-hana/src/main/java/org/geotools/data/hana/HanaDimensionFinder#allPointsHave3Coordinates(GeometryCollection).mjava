    private static boolean allPointsHave3Coordinates(GeometryCollection gc) {
        int numGeometries = gc.getNumGeometries();
        for (int i = 0; i < numGeometries; ++i) {
            if (!allPointsHave3Coordinates(gc.getGeometryN(i))) {
                return false;
            }
        }
        return true;
    }

