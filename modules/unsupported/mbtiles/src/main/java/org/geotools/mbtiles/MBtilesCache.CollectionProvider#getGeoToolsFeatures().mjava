        public SimpleFeatureCollection getGeoToolsFeatures() {
            if (converted == null) {
                synchronized (this) {
                    if (converted == null) {
                        mvtFeatures.stream().forEach(f -> builder.addFeature(f));
                        converted = builder.result;
                        builder = null;
                        mvtFeatures = null;
                    }
                }
            }

            return converted;
        }

