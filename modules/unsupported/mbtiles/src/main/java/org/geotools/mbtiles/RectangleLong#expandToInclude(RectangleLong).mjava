    public void expandToInclude(RectangleLong other) {
        if (isNull()) {
            init(other);
        } else {
            if (other.minX < minX) {
                minX = other.minX;
            }
            if (other.maxX > maxX) {
                maxX = other.maxX;
            }
            if (other.minY < minY) {
                minY = other.minY;
            }
            if (other.maxY > maxY) {
                maxY = other.maxY;
            }
        }
    }

