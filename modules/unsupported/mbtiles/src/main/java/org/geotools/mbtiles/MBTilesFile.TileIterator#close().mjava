        @Override
        @SuppressWarnings("PMD.UseTryWithResources") // field closure
        public void close() throws IOException {
            try {
                try {
                    rs.close();
                } finally {
                    try {
                        st.close();
                    } finally {
                        cx.close();
                    }
                }
            } catch (SQLException e) {
                throw new IOException(e);
            }
        }

