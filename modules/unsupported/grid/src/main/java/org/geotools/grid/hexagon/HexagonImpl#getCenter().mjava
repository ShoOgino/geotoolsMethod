    /**
     * {@inheritDoc}
     */
    public Coordinate getCenter() {
        if (orientation == Orientation.FLAT) {
            return new Coordinate(minX + sideLen, minY + ROOT3 * 0.5 * sideLen);
        } else {  // ANGLED
            return new Coordinate(minX + ROOT3 * 0.5 * sideLen, minY + sideLen);
        }
    }

