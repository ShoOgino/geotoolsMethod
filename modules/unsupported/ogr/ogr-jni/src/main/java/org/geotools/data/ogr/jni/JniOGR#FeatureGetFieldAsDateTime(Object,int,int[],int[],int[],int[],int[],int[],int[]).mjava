    @Override
    public void FeatureGetFieldAsDateTime(
            Object feature,
            int i,
            int[] year,
            int[] month,
            int[] day,
            int[] hour,
            int[] minute,
            int[] second,
            int[] tzFlag) {
        float[] secondFloat = new float[second.length];
        ((Feature) feature)
                .GetFieldAsDateTime(i, year, month, day, hour, minute, secondFloat, tzFlag);
        for (int j = 0; j < second.length; j++) {
            second[j] = (int) secondFloat[j];
        }
    }

