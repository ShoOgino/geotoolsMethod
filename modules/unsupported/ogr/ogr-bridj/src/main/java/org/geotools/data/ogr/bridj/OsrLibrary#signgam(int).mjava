    /// This variable is used by `gamma' and `lgamma'.
    public OsrLibrary signgam(int signgam) {
        try {
            {
                BridJ.getNativeLibrary("osr")
                        .getSymbolPointer("signgam")
                        .as(int.class)
                        .set(signgam);
                return this;
            }
        } catch (Throwable $ex$) {
            throw new RuntimeException($ex$);
        }
    }

