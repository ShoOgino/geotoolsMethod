    /**
     * Override which checks if the fixture is available. If not the test is not
     * executed.
     */
    @Override
    public void run(TestResult result) {
        if (gdalAvailable()) {
            super.run(result);
        } else {
            System.out.println("Skipping tests " + result.toString() + " since GDAL is not available");
        }
    }

