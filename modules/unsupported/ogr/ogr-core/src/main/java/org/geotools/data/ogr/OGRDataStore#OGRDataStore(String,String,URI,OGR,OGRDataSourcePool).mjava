    public OGRDataStore(
            String ogrName,
            String ogrDriver,
            URI namespace,
            OGR ogr,
            OGRDataSourcePool dataSourcePool) {
        if (namespace != null) {
            setNamespaceURI(namespace.toString());
        }
        this.ogrSourceName = ogrName;
        this.ogrDriver = ogrDriver;
        this.ogr = ogr;
        this.dataSourcePool = dataSourcePool;
    }

