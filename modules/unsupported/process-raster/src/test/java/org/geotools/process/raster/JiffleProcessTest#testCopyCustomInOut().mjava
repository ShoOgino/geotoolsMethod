    @Test
    public void testCopyCustomInOut() {
        assertCopy(
                inputs -> {
                    inputs.put(JiffleProcess.IN_DEST_NAME, "result");
                    inputs.put(JiffleProcess.IN_SOURCE_NAME, "abc");
                    inputs.put(JiffleProcess.IN_SCRIPT, "result = abc;");
                });
    }

