    /*
    Extract srs Id from xml configuration file
     */
    protected Integer getSrsId() {
        String crsStr = getConfig().getCoordsys();
        String[] crsComponents = crsStr.split(":");
        if (crsComponents.length == 2) {
            try {
                return Integer.valueOf(crsComponents[1]);
            } catch (Exception e) {
                return null;
            }
        }
        return null;
    }

