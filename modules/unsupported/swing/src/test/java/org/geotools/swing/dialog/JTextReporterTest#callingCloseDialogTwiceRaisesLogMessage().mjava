    @Test
    public void callingCloseDialogTwiceRaisesLogMessage() throws Exception {
        Connection conn = showDialog(TITLE).get();
        conn.closeDialog();
        windowFixture.robot.waitForIdle();
        captureLogger();

        conn.closeDialog();
        assertLogMessage("INFO");
        assertLogMessage("connection has expired");

        releaseLogger();
    }

