    @Test
    public void testRemoveAt() {
        DnDListModel<String> model = new DnDListModel<>();
        model.addItem("one");
        model.addItem("two");
        model.addItem("three");
        model.removeAt(1);

        assertEquals("one", model.getElementAt(0));
        assertEquals("three", model.getElementAt(1));
    }

