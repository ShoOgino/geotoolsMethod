    /** Add new items to the end of the list of current items */
    public void addItems(T[] newItems) {
        if (newItems.length > 0) {
            int index0 = items.size();

            for (T item : newItems) {
                items.add(item);
            }

            if (notify) {
                fireIntervalAdded(this, index0, index0 + newItems.length - 1);
            }
        }
    }

