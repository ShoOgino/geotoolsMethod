    /**
     * Retrieve the specified column name
     *
     * @param column column index
     *
     * @return the column name
     */
    @Override
    public String getColumnName(int column) {
    	if( column == 0 ){
    		return "FeatureIdentifer";
    	}
        return schema.getDescriptor( column-1 ).getLocalName();
    }

