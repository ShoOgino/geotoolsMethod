    public JDataStorePage(DataStoreFactorySpi format, Map<String, Object> params) {
        this.format = format;
        if (params == null) {
            params = new HashMap<>();
            if (format != null) {
                for (Param param : format.getParametersInfo()) {
                    params.put(param.key, param.sample);
                }
            }
        }
        this.connectionParameters = params;
    }

