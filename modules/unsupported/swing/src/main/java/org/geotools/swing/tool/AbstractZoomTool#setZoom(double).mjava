    /**
     * Set the zoom increment
     *
     * @param newZoom the new zoom increment; values &lt;= 1.0 will be ignored
     * @return the previous zoom increment
     */
    public double setZoom(double newZoom) {
        double old = zoom;
        if (newZoom > 1.0d) {
            zoom = newZoom;
        }
        return old;
    }

