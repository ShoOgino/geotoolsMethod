    @Override
    public File getValue() {
        String txt = field.getText();
        if (txt == null || txt.equals("")) {
            return null;
        }
        try {
            File file = new File(txt);
            return file;
        } catch (Exception e) {
        }
        try {
            URL url = new URL(txt);
            return URLs.urlToFile(url);
        } catch (MalformedURLException e) {
        }
        return null; // not a file
    }

