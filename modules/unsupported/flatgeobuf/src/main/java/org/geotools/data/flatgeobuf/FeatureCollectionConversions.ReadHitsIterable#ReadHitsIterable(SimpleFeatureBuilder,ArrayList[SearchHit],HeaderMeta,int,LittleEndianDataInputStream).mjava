        private ReadHitsIterable(
                SimpleFeatureBuilder fb,
                ArrayList<SearchHit> hits,
                HeaderMeta headerMeta,
                int featuresOffset,
                LittleEndianDataInputStream data) {
            this.fb = fb;
            this.hits = hits;
            this.headerMeta = headerMeta;
            this.featuresOffset = featuresOffset;
            this.data = data;
        }

