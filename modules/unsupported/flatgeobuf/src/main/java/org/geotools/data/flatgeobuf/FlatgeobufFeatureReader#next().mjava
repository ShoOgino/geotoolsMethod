    @Override
    public SimpleFeature next()
            throws IOException, IllegalArgumentException, NoSuchElementException {
        SimpleFeature feature = null;
        if (nextFeature != null) {
            feature = nextFeature;
            nextFeature = null;
        } else {
            feature = getFlatgeobufReader().getNextFeature();
            featureIndex++;
        }
        return feature;
    }

