	public void setUpForReading(File argTarget)
	{
		SAXBuilder builder = new SAXBuilder();
		
		try 
		{
			targetDoc = builder.build(argTarget);
		} 
		
		catch (JDOMException caught) 
		{
			caught.printStackTrace();
		} 
		
		catch (IOException caught) 
		{
			caught.printStackTrace();
		}
	}

