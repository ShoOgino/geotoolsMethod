    @Override
    public EngineeringCRS createEngineeringCRS(String code)
            throws NoSuchAuthorityCodeException, FactoryException {
        if (DISPLAY_CODE.equals(code) || ("EPSG:" + DISPLAY_CODE).equals(code)) {
            return DISPLAY;
        } else {
            throw noSuchAuthorityException(code);
        }
    }

