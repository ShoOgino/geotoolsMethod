    /**
     * @return {@code true} if fid can be parsed to a long, as OID's are longs
     * @see FIDMapper#isValid(String)
     */
    public boolean isValid(String fid) {
        try{
            Long.parseLong(fid, 10);
        }catch(NumberFormatException e){
            return false;
        }
        return true;
    }

