    @SuppressWarnings("unchecked")
    protected static EFeaturePropertyDelegate<?, ? extends Property, ? extends EStructuralFeature>
            newProperty(EFeatureInternal eInternal, String eName, Class<?> type) {
        if (Geometry.class.isAssignableFrom(type)) {
            return newGeometry(eInternal, eName, (Class<? extends Geometry>) type);
        }
        return newAttribute(eInternal, eName, type);
    }

