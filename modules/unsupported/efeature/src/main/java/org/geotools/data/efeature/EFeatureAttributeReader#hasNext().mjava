    @Override
    public boolean hasNext() throws IOException {
        if (eNext != null) {
            return true;
        }
        if (eIterator == null) {
            // Get EFeature iterator
            //
            eIterator = eQuery.iterator();
        }
        if (eIterator.hasNext()) {
            eNext = eIterator.next();
        }
        return eNext != null;
    }

