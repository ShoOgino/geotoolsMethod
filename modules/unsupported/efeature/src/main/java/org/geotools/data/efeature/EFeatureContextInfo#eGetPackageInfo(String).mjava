    /**
     * Get {@link EFeaturePackageInfo} with given {@link EPackage#getNsURI() namespace URI}.
     *
     * @param eNsURI
     * @throws IllegalArgumentException If not found
     * @throws IllegalStateException If {@link #isDisposed() disposed}.
     */
    public EFeaturePackageInfo eGetPackageInfo(String eNsURI) {
        verify();
        EFeaturePackageInfo eInfo = synchronize(this).get(eNsURI);
        if (eInfo == null) {
            throw new IllegalArgumentException("EFeaturePackageInfo [" + eNsURI + "] not found");
        }
        return eInfo;
    }

