    @Override
    protected void doInvalidate(boolean deep) {
        //
        // Dispose weak references
        //
        this.eParentClass = null;
        this.eReference = null;
        //
        // Do a deep invalidation?
        //
        if (deep) {
            for (EStructureInfo<?> it : eGetAttributeInfoList(true)) {
                it.doInvalidate(true);
            }
        }
    }

