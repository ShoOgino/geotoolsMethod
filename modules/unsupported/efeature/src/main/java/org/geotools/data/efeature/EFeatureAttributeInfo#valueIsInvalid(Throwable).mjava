    protected final EFeatureStatus valueIsInvalid(Throwable cause) {
        StackTraceElement trace = EFeatureUtils.getStackTraceElement(0, 1);
        return failure(
                this,
                eName(),
                "Attribute value is invalid: "
                        + trace.getClassName()
                        + "["
                        + eName()
                        + "]#"
                        + trace.getMethodName());
    }

