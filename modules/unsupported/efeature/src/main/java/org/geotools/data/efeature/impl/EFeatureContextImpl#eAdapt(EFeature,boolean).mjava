    @Override
    public EFeatureInfo eAdapt(EFeature eFeature, boolean copy) {
        //
        // Forward to structure
        //
        EFeatureInfo eInfo = eStructure().eAdapt(eFeature.getStructure(), copy);
        //
        // Update structure
        //
        eFeature.setStructure(eInfo);
        //
        // Finished
        //
        return eInfo;
    }

