    protected String eGetID(URI eURI, EObject eImpl) {
        //
        // Get map
        //
        Map<EObject, Long> eIDs = eInverseIDMap.get(eURI);
        //
        // Locate ID if exists?
        //
        if (eIDs != null) {
            Long uniqueID = eIDs.get(eImpl);
            if (uniqueID != null) {
                return ePrefix + uniqueID;
            }
        }
        return null;
    }

