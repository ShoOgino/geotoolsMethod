    @Override
    public void release() {
        //
        // Remove adapters
        //
        eFeature.eAdapters().remove(eObjectlistener);
        //
        // Remove structure listeners
        //
        eStructure.removeListener(eStructurelistener);
        //
        // Release all strong references to external objects
        //
        eFeature = null;
        eStructure = null;
        eInternal = null;
        eTx = null;
    }

