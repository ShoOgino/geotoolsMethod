    public String getID(URI eURI, EObject eObject) {
        //
        // Verify URI
        //
        verify("eURI", eURI, true);
        //
        // Verify EObject
        //
        verify("eObject", eObject, true, false, true);
        //
        // Locate ID if exists
        //
        for(Map<EObject,Long> it : eCachedIDMap.values()) {
            Long uniqueID = it.get(eObject);
            if(uniqueID!=null) {
                return ePrefix + uniqueID;
            }
        }       
        return null;
    }

