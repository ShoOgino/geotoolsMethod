    private static void assertWKB(byte[] wkb) throws ParseException {
        Geometry g = DataBuilder.toGeometry(wkb);
        byte[] b = WKBAdapter.DEFAULT.toWKB(g);
        assertTrue(
                "WKB: [" + Arrays.toString(b) + "] is not equal to [" + Arrays.toString(wkb) + "]",
                Arrays.equals(wkb, b));
    }

