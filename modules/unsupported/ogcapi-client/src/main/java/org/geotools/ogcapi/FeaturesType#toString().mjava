    @Override
    public String toString() {
        StringBuilder sb = new StringBuilder();
        sb.append("OGCAPI Features:\n")
                .append("\tTitle: " + title + "\n")
                .append("\tDesc: " + description + "\n");
        sb.append("\tLinks:\n");
        for (Link l : links) {
            sb.append("\t\t" + l.toString()).append("\n");
        }
        return sb.toString();
    }

