        @Override
        public boolean hasNext() {
            if (next != null) {
                return true;
            }

            if (handler == null) {
                handler = new FeatureCollectionHandler(featureType, attio);
                // handler = GeoJSONUtil.trace(handler, IFeatureCollectionHandler.class);
            }
            next = readNext();
            return next != null;
        }

