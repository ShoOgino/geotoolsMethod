    @Override
    protected MultiLineString createGeometry() {
        return gf.createMultiLineString(
                new LineString[] {
                    gf.createLineString(
                            new Coordinate[] {
                                new Coordinate(100.0, 0.0), new Coordinate(101.0, 1.0)
                            }),
                    gf.createLineString(
                            new Coordinate[] {
                                new Coordinate(102.0, 2.0), new Coordinate(103.0, 3.0)
                            })
                });
    }

