    @Test
    public void testParsePolygon() throws ParseException {
        String line =
                "{\n"
                        + "         \"type\": \"Polygon\",\n"
                        + "         \"coordinates\": [\n"
                        + "             [\n"
                        + "                 [100.0, 0.0],\n"
                        + "                 [101.0, 0.0],\n"
                        + "                 [101.0, 1.0],\n"
                        + "                 [100.0, 1.0],\n"
                        + "                 [100.0, 0.0]\n"
                        + "             ],\n"
                        + "             [\n"
                        + "                 [100.8, 0.8],\n"
                        + "                 [100.8, 0.2],\n"
                        + "                 [100.2, 0.2],\n"
                        + "                 [100.2, 0.8],\n"
                        + "                 [100.8, 0.8]\n"
                        + "             ]\n"
                        + "         ]\n"
                        + "     }";
        String expected =
                "POLYGON ((100 0, 101 0, 101 1, 100 1, 100 0), (100.8 0.8, 100.8 0.2, 100.2 0.2, 100.2 0.8, 100.8 0.8))";
        testGeometry(line, expected);
    }

