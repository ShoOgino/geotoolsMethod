    @Override
    @SuppressWarnings("PMD.UseTryWithResources") // closing field
    public void close() throws IOException {
        try {
            if (inArray) {
                generator.writeEndArray();
                if (isEncodeFeatureBounds() && notWritenBbox) {
                    writeBoundingEnvelope();
                }
                generator.writeEndObject();
            }

            generator.close();
        } finally {
            out.close();
        }
    }

