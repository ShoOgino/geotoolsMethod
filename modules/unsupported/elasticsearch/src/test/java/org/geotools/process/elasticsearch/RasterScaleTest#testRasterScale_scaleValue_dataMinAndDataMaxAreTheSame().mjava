    @Test
    public void testRasterScale_scaleValue_dataMinAndDataMaxAreTheSame() {
        float scaleMax = 10.0f;
        float scaleMin = 1.0f;
        RasterScale scale = new RasterScale(scaleMin, scaleMax);
        scale.prepareScale(30);
        assertEquals(10, scale.scaleValue(30), 0.0);
    }

