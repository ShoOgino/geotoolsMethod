    @Test
    public void testCompare() {     
      other = new ElasticAttribute("other");     
      attr.setOrder(1);
      other.setOrder(2);
      assertEquals(-1, attr.compareTo(other));
      attr.setOrder(3);
      other.setOrder(2);
      assertEquals(1, attr.compareTo(other));
      attr.setOrder(null);
      other.setOrder(1);
      assertEquals(1, attr.compareTo(other));
      attr.setOrder(1);
      other.setOrder(null);
      assertEquals(-1, attr.compareTo(other));
      other = new ElasticAttribute("zAfter");
      attr.setOrder(null);
      other.setOrder(null);
      assertTrue(attr.compareTo(other) < 0);
      other = new ElasticAttribute("before");
      attr.setOrder(1);
      other.setOrder(1);
      assertTrue(attr.compareTo(other) > 0); 
    }

