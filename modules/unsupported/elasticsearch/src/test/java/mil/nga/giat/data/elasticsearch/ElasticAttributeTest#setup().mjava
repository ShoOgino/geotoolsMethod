    @Before
    public void setup() {
        name = "theName";
        attr = new ElasticAttribute(name);
        shortName = "name";
        useShortName = true;
        customName = "XML Custom Name";
        normalizedName = "_XML_Custom_Name";
        type = Map.class;
        geometryType = ElasticGeometryType.GEO_SHAPE;
        use = true;
        defaultGeometry = true;
        srid = 10;
        order = 1;
        dateFormat = "yyyy-mm-dd";
        analyzed = true;
        stored = true;
        nested = true;
    }

