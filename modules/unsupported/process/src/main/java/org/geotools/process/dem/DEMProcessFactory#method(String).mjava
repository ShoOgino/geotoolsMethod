    public Method method( String name) {
        for( Method method : TARGET.getMethods() ){
            DescribeProcess INFO = method.getAnnotation( DescribeProcess.class );
            if( INFO != null ){
                if( name.equalsIgnoreCase( method.getName() )){
                    return method;
                }
            }
        }
        return null;
    }
