  @Override
  public boolean equals(Object other) {
    if (other == this) {
      return true;
    }
    if ((other instanceof EquipmentType) == false) {
      return false;
    }
    EquipmentType rhs = ((EquipmentType) other);
    return new EqualsBuilder().append(code, rhs.code).append(description, rhs.description)
        .append(idNumber, rhs.idNumber).append(additionalProperties, rhs.additionalProperties).isEquals();
  }

