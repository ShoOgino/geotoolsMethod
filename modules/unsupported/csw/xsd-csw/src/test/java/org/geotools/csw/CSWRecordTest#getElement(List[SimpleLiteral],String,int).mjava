    private Object getElement(List<SimpleLiteral> elements, String name, int idx) {
        int curr = 0;
        for (SimpleLiteral sl : elements) {
            if(name.equals(sl.getName())) {
                if(idx == curr) {
                    return sl.getValue();
                } else {
                    curr++;
                }
            }
        }
        
        return null;
    }

