        public SLDSelector(NumberRange<?> scaleRange, Filter filter) {
            this.scaleRange =
                    new NumberRange<>(
                            Double.class,
                            scaleRange.getMinimum(),
                            scaleRange.isMinIncluded(),
                            scaleRange.getMaximum(),
                            scaleRange.isMaxIncluded());
            this.filter = filter;
        }

