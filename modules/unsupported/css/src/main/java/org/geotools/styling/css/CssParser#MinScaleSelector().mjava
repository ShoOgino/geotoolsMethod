    Rule MinScaleSelector() {
        return Sequence(
                "[",
                OptionalWhiteSpace(),
                FirstOf("@scale", "@sd"),
                OptionalWhiteSpace(),
                FirstOf(">=", ">"),
                OptionalWhiteSpace(),
                ScaleNumber(),
                push(new ScaleRange(parseScaleValue(match()), true, Double.POSITIVE_INFINITY, true)), //
                OptionalWhiteSpace(), "]");
    }

