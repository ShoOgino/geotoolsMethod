    Rule MinScaleSelector() {
        return Sequence(
                "[",
                OptionalWhiteSpace(),
                "@scale",
                OptionalWhiteSpace(),
                FirstOf(">=", ">"),
                OptionalWhiteSpace(),
                ScaleValue(),
                push(new ScaleRange(parseScaleValue(match()), true, Double.POSITIVE_INFINITY, true)), //
                OptionalWhiteSpace(), "]");
    }

