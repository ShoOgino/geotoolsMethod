    /**
     * Returns a new Specificity object representing by performing a element by element sum of the
     * components
     */
    public Specificity sum(Specificity other) {
        if (this == ZERO) {
            return other;
        } else if (other == ZERO) {
            return this;
        }
        return new Specificity(b + other.b, c + other.c, d + other.d);
    }

