    Rule MaxScaleSelector() {
        return Sequence(
                "[",
                OptionalWhiteSpace(),
                FirstOf("@scale", "@sd"),
                OptionalWhiteSpace(),
                FirstOf("<=", "<"),
                OptionalWhiteSpace(),
                ScaleNumber(),
                push(new ScaleRange(0, true, parseScaleValue(match()), false)), //
                OptionalWhiteSpace(),
                "]");
    }

