    /**
     * See if a certain signature matches an already rejected signature
     * 
     * @param s
     * @param k
     * @return
     */
    private boolean rejected(Signature s, int k) {
        // see if rejected already
        for (Signature reject : rejects) {
            if (s.contains(reject, k)) {
                return true;
            }
        }

        return false;
    }

