    public int getMaxMultiValueSize(Map<String, List<Value>> valueMap, int i, String... names) {
        int max = 0;
        for (String name : names) {
            List<Value> values = getMultiValue(valueMap, name, i);
            int size = values.size();
            if (size > max) {
                max = size;
            }
        }
        return max;
    }

