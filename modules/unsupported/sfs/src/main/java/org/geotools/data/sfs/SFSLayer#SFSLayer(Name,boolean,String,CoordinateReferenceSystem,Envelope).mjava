    public SFSLayer(
            Name typeName,
            boolean layerAxisOrder,
            String layerSRS,
            CoordinateReferenceSystem crs,
            Envelope bounds) {
        super();
        this.typeName = typeName;
        this.xyOrder = layerAxisOrder;
        this.layerSRS = layerSRS;
        this.coordinateReferenceSystem = crs;
        this.bounds = bounds;
    }

