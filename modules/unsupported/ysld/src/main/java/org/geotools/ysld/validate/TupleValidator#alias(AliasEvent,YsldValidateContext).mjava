    @Override
    public void alias(AliasEvent evt, YsldValidateContext context) {
        switch(state) {
        case NEW:
            // Can't validate this so just move on.
            state = State.DONE;
            context.pop();
            break;
        case STARTED:
            // Can't validate this so just move on.
            valuesValidated++;
            break;
        default:
            context.error(String.format("Unexpected alias '%s'", evt.getAnchor()), evt.getStartMark());
            break;
        }
    }

