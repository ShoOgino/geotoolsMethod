    /**
     * YamlSeq access
     * 
     * @param i index
     * @return Sequence access, provided as {@link YamlSeq} wrapper
     */
    public YamlSeq seq(int i) {
        Object obj = raw.get(i);
        if (obj == null) {
            return null;
        }
        if (obj instanceof YamlSeq) {
            return (YamlSeq) obj;
        }
        return new YamlSeq(obj);
    }

