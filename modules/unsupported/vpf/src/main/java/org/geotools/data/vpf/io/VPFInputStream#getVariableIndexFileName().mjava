    /**
     * Describe <code>getVariableIndexFileName</code> method here.
     *
     * @return a <code>String</code> value
     */
    public String getVariableIndexFileName() {
        if (streamFile.equals("fcs")) {
            return "fcz";
        } else {
            return streamFile.substring(0, streamFile.length() - 1) + "x";
        }
    }

