    /**
     * Describe <code>toString</code> method here.
     *
     * @return a <code>String</code> value
     */
    public String toString() {
        StringBuffer sb = new StringBuffer(dateBin.length);

        for (int i = 0; i < dateBin.length; i++) {
            sb.append((char) dateBin[i]);
        }

        return sb.toString();
    }

