    /**
     * Describe <code>getCalendar</code> method here.
     *
     * @return a <code>Calendar</code> value
     */
    public Calendar getCalendar() {
        try {
            sdf.parse(toString());

            return sdf.getCalendar();
        } catch (ParseException e) {
            LOGGER.log(Level.SEVERE, "", e);
        }

        return null;
    }

