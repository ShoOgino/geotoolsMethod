    private static Geometry parseFourDimensionGeometry(
            String type, List<List<List<List<Double>>>> coords) throws ParseException {
        Geometry result;
        switch (type) {
            case "MultiPolygon":
                result = parseMultiPolygon(coords);
                break;
            default:
                throw new ParseException("Invalid geometry type: " + type);
        }

        return result;
    }

