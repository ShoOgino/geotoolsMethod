    public boolean intersectInteriorInteriorPoint() {
        /**
         * the interiors of the segments intersect at a node if both itscs are real number not equal
         * 0.0 and not equal 1.0
         */
        if ((itsc == null) || (itsc.length != 2)) return false;
        return ((!isNaN(itsc[0]) && itsc[0] != 0.0 && itsc[0] != 1.0)
                && (!isNaN(itsc[1]) && itsc[1] != 0.0 && itsc[1] != 1.0));
    }

