	/**
	 * Returns whether #equals returns true for any two consecutive Coordinates
	 * in the given array.
	 */
	public static boolean hasRepeatedPoints(Coordinate[] coord) {
		for (int i = 1; i < coord.length; i++) {
			if (coord[i - 1].equals(coord[i])) {
				return true;
			}
		}
		return false;
	}

