	/**
	 * 
	 * @param edgeSet
	 * @param x
	 * @param insertEvent
	 * @param obj
	 */
	public SweepLineEvent(Object edgeSet, double x, SweepLineEvent insertEvent,
			Object obj) {
		this.edgeSet = edgeSet;
		xValue = x;
		this.insertEvent = insertEvent;
		this.eventType = INSERT;
		if (insertEvent != null)
			eventType = DELETE;
		this.obj = obj;
	}

