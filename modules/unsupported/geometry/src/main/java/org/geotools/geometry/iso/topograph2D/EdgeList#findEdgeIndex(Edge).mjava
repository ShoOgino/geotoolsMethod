    /**
     * If the edge e is already in the list, return its index.
     *
     * @return index, if e is already in the list -1 otherwise
     */
    public int findEdgeIndex(Edge e) {
        for (int i = 0; i < edges.size(); i++) {
            if (((Edge) edges.get(i)).equals(e)) return i;
        }
        return -1;
    }

