    private String getTopologySummary() {
        StringBuffer catBuf = new StringBuffer();
        if (isEndPoint()) catBuf.append(" endpoint");
        if (isProper) catBuf.append(" proper");
        if (isCollinear()) catBuf.append(" collinear");
        return catBuf.toString();
    }

