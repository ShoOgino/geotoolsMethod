    /**
     * Returns the division of two doubles: d1 / d2
     *
     * @param d1 Dividend
     * @param d2 Divisor
     * @return Division of the two values
     */
    public static double div(double d1, double d2) {
        return (BigDecimal.valueOf(d1)).divide(BigDecimal.valueOf(d2)).doubleValue();
    }

