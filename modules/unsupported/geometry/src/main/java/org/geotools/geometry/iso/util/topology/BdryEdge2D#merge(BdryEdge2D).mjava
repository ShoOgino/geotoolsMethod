    public void merge(BdryEdge2D other) {
        BRepNode2D n;
        if (this.p1 == other.p1) {
            n = this.p1;
        } else if (this.p1 == other.p2) {
            n = this.p1;
        } else if (this.p2 == other.p1) {
            n = this.p2;
        } else if (this.p2 == other.p2) {
            n = this.p2;
        } else return;

        if (n.edges.size() != 2) return;
    }

