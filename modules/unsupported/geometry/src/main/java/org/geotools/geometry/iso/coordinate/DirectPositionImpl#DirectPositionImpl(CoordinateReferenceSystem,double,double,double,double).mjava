    /**
     * @param crs
     * @param x
     * @param y
     * @param z
     * @param m
     */
    public DirectPositionImpl(
            CoordinateReferenceSystem crs, double x, double y, double z, double m) {
        this.crs = crs;
        assert (5 == crs.getCoordinateSystem().getDimension());
        this.coordinate = new double[] {x, y, z, m};
    }

