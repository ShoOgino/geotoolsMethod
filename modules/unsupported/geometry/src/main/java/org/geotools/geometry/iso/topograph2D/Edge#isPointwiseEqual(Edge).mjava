	/**
	 * @return true if the coordinate sequences of the Edges are identical
	 */
	public boolean isPointwiseEqual(Edge e) {
		if (pts.length != e.pts.length)
			return false;

		for (int i = 0; i < pts.length; i++) {
			if (!pts[i].equals2D(e.pts[i])) {
				return false;
			}
		}
		return true;
	}

