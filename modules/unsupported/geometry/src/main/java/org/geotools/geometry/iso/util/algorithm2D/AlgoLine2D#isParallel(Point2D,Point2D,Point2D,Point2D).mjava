    public static boolean isParallel(Point2D p0, Point2D p1, Point2D q0, Point2D q1) {
        return Math.abs(
                        (double)
                                AlgoPoint2D.cross(
                                        AlgoPoint2D.subtract(p1, p0), AlgoPoint2D.subtract(q1, q0)))
                <= AlgoPoint2D.EPSILON;
    }

