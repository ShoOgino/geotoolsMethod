    /**
     * Adds a coordinate to another coordinate
     *
     * @return double[]
     */
    public static double[] add(double[] p0, double[] p1) {
        // Test OK
        int n = p0.length;
        double result[] = new double[n];
        for (int i = 0; i < n; ++i) {
            result[i] = p0[i] + p1[i];
        }
        return result;
    }

