    /** @see FeatureReader#close() */
    public void close() throws IOException {
        // System.err.println("Closing WFSFeatureReader for " + featureType.getName());
        final GetParser<SimpleFeature> parser = this.parser;
        this.parser = null;
        this.next = null;
        if (parser != null) {
            parser.close();
        }
    }

