    /**
     * @see WFSDataStore#getCapabilitiesURL()
     */
    public URL getCapabilitiesURL() {
        URL capsUrl = wfs.getOperationURL(GET_CAPABILITIES, false);
        if (capsUrl == null) {
            capsUrl = wfs.getOperationURL(GET_CAPABILITIES, true);
        }
        return capsUrl;
    }

