    @Override
    public Feature next() throws NoSuchElementException {
        if (!hasNextCalled) {
            if (hasNext()) {
                this.hasNextCalled = false;
                return parsedFeature;
            } else {
                close();
                return null;
            }
        } else {
            this.hasNextCalled = false;
            return parsedFeature;
        }
    }

