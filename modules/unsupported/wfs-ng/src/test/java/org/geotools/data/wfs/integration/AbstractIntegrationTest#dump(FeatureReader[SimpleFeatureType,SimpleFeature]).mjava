    void dump(FeatureReader<SimpleFeatureType, SimpleFeature> reader) throws Exception {
        SimpleFeature feature;
        int count = 0;

        try {
            while (reader.hasNext()) {
                feature = reader.next();
                LOGGER.fine(count + " feature:" + feature);
                count++;
            }
        } finally {
            reader.close();
        }
    }

