    public Class type(String string) {
        switch (string) {
            case "array":
                return Collection.class;
            case "boolean":
                return Boolean.class;
            case "number":
                return Number.class;
            case "object":
                return JSONObject.class;
            case "string":
                return String.class;
        }
        throw new IllegalArgumentException("Requires argument of array, boolean, number, object or string");
    }

