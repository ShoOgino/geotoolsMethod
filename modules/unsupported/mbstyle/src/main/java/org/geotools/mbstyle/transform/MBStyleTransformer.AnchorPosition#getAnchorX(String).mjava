        public Double getAnchorX(String jsonString) {
            AnchorPosition pos = AnchorPosition.valueOf(jsonString);
            Double x = 0.0;
            switch (pos) {
                case BOTTOM:
                    x = 0.5;
                    break;
                case BOTTOM_LEFT:
                    x = 0.0;
                    break;
                case BOTTOM_RIGHT:
                    x = 1.0;
                    break;
                case CENTER:
                    x = 0.5;
                    break;
                case LEFT:
                    x = 0.0;
                    break;
                case RIGHT:
                    x = 1.0;
                    break;
                case TOP:
                    x = 0.5;
                    break;
                case TOP_LEFT:
                    x = 0.0;
                    break;
                case TOP_RIGHT:
                    x = 1.0;
                    break;
            }
            return x;
        }

