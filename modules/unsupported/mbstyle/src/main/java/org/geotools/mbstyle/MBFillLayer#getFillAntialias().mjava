    /**
     * (Optional) Whether or not the fill should be antialiased.
     * 
     * Defaults to true.
     * 
     */
    public Boolean getFillAntialias() {
        if (paintJson.get("fill-antialias") != null) {
            return (Boolean) paintJson.get("fill-antialias");
        } else {
            return true;
        }
    }

