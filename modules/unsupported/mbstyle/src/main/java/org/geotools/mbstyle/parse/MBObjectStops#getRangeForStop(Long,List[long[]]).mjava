    public static long[] getRangeForStop(Long stop, List<long[]> ranges) {
        long[] rangeForStopLevel = {0,0};
        for (int i = 0; i < ranges.size(); i++) {
            if (ranges.get(i)[0] == stop) {
                rangeForStopLevel = ranges.get(i);
            }
        }
        return rangeForStopLevel;
    }    

