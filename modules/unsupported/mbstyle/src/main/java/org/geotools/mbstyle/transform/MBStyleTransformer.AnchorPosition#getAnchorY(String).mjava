        public Double getAnchorY(String jsonString) {
            AnchorPosition pos = AnchorPosition.valueOf(jsonString);
            Double y = 0.0;
            switch (pos) {
                case BOTTOM:
                    y = 0.0;
                    break;
                case BOTTOM_LEFT:
                    y = 0.0;
                    break;
                case BOTTOM_RIGHT:
                    y = 0.0;
                    break;
                case CENTER:
                    y = 0.5;
                    break;
                case LEFT:
                    y = 0.5;
                    break;
                case RIGHT:
                    y = 0.5;
                    break;
                case TOP:
                    y = 1.0;
                    break;
                case TOP_LEFT:
                    y = 1.0;
                    break;
                case TOP_RIGHT:
                    y = 1.0;
                    break;
            }
            return y;
        }

