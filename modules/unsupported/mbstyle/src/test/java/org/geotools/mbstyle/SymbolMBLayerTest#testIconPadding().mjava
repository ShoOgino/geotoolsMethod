    @Test
    public void testIconPadding() {
        assertEquals(30.0, testLayer.getIconPadding());
        // defaults to 2.0
        assertEquals(2.0, testLayerDefault.getIconPadding());
        assertEquals(
                "20.0",
                featureTypePoint
                        .get(0)
                        .rules()
                        .get(0)
                        .getSymbolizers()[1]
                        .getOptions()
                        .get("spaceAround"));
        assertEquals(30.0, testLineLayer.getIconPadding());
        assertEquals(
                "15.0",
                featureTypeLine
                        .get(0)
                        .rules()
                        .get(0)
                        .getSymbolizers()[0]
                        .getOptions()
                        .get("spaceAround"));
    }

