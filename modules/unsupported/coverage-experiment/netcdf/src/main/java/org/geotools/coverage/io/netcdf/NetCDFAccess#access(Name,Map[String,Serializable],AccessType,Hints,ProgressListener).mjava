    public CoverageSource access(Name name, Map<String, Serializable> params,
            AccessType accessType, Hints hints, ProgressListener listener)
            throws IOException {
        if (listener == null)
            listener = new NullProgressListener();
        listener.started();
        try {
            return new NetCDFSource(this, name);
        } finally {
            listener.complete();
        }
    }

