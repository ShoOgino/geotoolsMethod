    /**
     * 
     * @param path
     * @param queryParams
     * @return
     * @throws IOException
     */
    public CouchDBResponse get(String path, NameValuePair[] queryParams) throws IOException {
        HttpMethod get = new GetMethod(url(path));
        if (queryParams != null) {
            get.setQueryString(queryParams);
        }
        return executeMethod(get);
    }

