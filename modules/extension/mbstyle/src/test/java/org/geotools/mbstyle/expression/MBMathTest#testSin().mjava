    @Test
    public void testSin() throws Exception {
        final JSONObject j = getObjectByLayerId("mathSin", "layout");
        for (int i = 0; i < intVals.length; ++i) {
            _assertEquals(
                    Math.sin(intVals[i]),
                    Double.parseDouble(
                            getExpressionEvaluation(j, "sin", testFeatures[i]).toString()));
        }
    }

