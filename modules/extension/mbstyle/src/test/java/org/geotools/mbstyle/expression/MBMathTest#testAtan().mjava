    @Test
    public void testAtan() throws Exception {
        final JSONObject j = getObjectByLayerId("mathAtan", "layout");
        for (int i = 0; i < intVals.length; ++i) {
            _assertEquals(
                    Math.atan(intVals[i]),
                    Double.parseDouble(
                            getExpressionEvaluation(j, "atan", testFeatures[i]).toString()));
        }
    }

