    @Override
    public AndBuilder<P> reset(And filter) {
        if (filter == null) {
            return unset();
        }
        this.list = new ArrayList<>();
        if (filter.getChildren() != null) {
            for (Filter child : filter.getChildren()) {
                list.add(new FilterBuilder().reset(child));
            }
        }
        return this;
    }

