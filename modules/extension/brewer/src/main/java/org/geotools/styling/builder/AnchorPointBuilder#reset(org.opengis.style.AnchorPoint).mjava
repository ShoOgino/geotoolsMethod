    public AnchorPointBuilder<P> reset(org.opengis.style.AnchorPoint anchorPoint) {
        if( anchorPoint == null ){
            return reset();
        }
        x.reset().literal(anchorPoint.getAnchorPointX());
        y.reset().literal(anchorPoint.getAnchorPointY());
        unset = false; 
        return this;
    }

