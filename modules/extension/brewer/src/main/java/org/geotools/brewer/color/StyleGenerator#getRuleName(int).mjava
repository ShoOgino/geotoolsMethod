    /**
     * Generates a quick name for each rule with a leading zero.
     *
     * @param count
     *
     */
    private static String getRuleName(int count) {
        String strVal = new Integer(count).toString();

        if (strVal.length() == 1) {
            return "rule0" + strVal;
        } else {
            return "rule" + strVal;
        }
    }

