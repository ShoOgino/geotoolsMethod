    @Override
    public ExternalMarkBuilder reset(ExternalMark mark) {
        if (mark == null) {
            return reset();
        }
        this.format = mark.getFormat();
        this.index = mark.getMarkIndex();
        this.inline = mark.getInlineContent();
        this.resource = mark.getOnlineResource();
        this.unset = false;

        return this;
    }

