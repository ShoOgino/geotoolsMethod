    /** Build an And filter */
    public And build() {
        if (list == null) {
            return null;
        }
        List<Filter> filters = new ArrayList<>(list.size());
        for (FilterBuilder build : list) {
            Filter filter = build.build();
            if (filter != null) {
                filters.add(filter);
            }
        }
        if (parent == null) {
            list.clear();
        }
        return ff.and(filters);
    }

