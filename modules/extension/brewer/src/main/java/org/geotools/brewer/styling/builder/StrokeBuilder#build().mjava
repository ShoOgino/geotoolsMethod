    @Override
    public Stroke build() {
        if (unset) {
            return null;
        }
        Stroke stroke =
                sf.createStroke(
                        color,
                        width,
                        opacity,
                        lineJoin,
                        lineCap,
                        dashArray,
                        dashOffset,
                        graphicFill.build(),
                        this.graphicStroke.build());
        if (dashArrayExpressions != null && !dashArrayExpressions.isEmpty()) {
            stroke.setDashArray(dashArrayExpressions);
        }
        if (parent == null) {
            reset();
        }
        return stroke;
    }

