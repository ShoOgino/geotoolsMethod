    private Filter partialIndexedFilterXpath() {
        FilterFactory2 ff = CommonFactoryFinder.getFilterFactory2();
        Filter filter =
                ff.and(
                        ff.equals(ff.property("st:Station"), ff.literal("st.1")),
                        ff.like(ff.property("st:Station/st:location/st:name"), "*fer*"));
        return filter;
    }

