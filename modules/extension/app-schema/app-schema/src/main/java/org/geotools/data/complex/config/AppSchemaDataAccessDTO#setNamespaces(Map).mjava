    /**
     * DOCUMENT ME!
     *
     * @param nameSpaces DOCUMENT ME!
     */
    public void setNamespaces(Map nameSpaces) {
        if (nameSpaces == null) {
            this.namespaces = Collections.EMPTY_MAP;
        } else {
            this.namespaces = new CheckedHashMap(String.class, String.class);
            this.namespaces.putAll(nameSpaces);
        }
    }

