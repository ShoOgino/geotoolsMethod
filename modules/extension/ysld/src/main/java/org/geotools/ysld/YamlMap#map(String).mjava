    /**
     * Access value as a YamlMap
     *
     * @param key
     * @return Access as YamlMap
     */
    public YamlMap map(String key) {
        Object obj = raw.get(key);
        if (obj == null) {
            return null;
        }
        if (obj instanceof YamlMap) {
            return (YamlMap) obj;
        }
        return new YamlMap(obj);
    }

