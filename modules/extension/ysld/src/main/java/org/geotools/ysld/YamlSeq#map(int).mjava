    /**
     * Map access
     *
     * @param i index
     * @return Map access, provided as {@link YamlMap} wrapper
     */
    public YamlMap map(int i) {
        Object obj = raw.get(i);
        if (obj == null) {
            return null;
        }
        if (obj instanceof YamlMap) {
            return (YamlMap) obj;
        }
        return new YamlMap(obj);
    }

