    /**
     * Should be used to encode values parsed with Util.name
     *
     * @param key
     * @param expr
     * @return
     */
    YsldEncodeHandler<T> putName(String key, Expression expr) {
        if (expr != null && expr != Expression.NIL) {
            put(key, toObjOrNull(expr, true));
        }
        return this;
    }

