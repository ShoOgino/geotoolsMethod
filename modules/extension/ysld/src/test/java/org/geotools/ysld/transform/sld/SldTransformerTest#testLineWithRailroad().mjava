        YamlMap style = transform("line", "railroad.sld");

        YamlMap line =
                style.seq("feature-styles")
                        .map(0)
                        .seq("rules")
                        .map(0)
                        .seq("symbolizers")
                        .map(1)
                        .map("line");
        YamlMap mark = line.map("stroke-graphic-stroke").seq("symbols").map(0).map("mark");
