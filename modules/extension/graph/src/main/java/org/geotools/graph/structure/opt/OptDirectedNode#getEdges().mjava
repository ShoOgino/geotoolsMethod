    /** @see Node#getEdges() */
    @Override
    public List<DirectedEdge> getEdges() {
        ArrayList<DirectedEdge> edges = new ArrayList<>();
        for (DirectedEdge edge : m_in) edges.add(edge);
        for (DirectedEdge directedEdge : m_out) edges.add(directedEdge);

        return (edges);
    }

