    /** @see Node#getEdges() */
    @Override
    public List<DirectedEdge> getEdges() {
        ArrayList<DirectedEdge> edges = new ArrayList<>();
        for (int i = 0; i < m_in.length; i++) edges.add(m_in[i]);
        for (int i = 0; i < m_out.length; i++) edges.add(m_out[i]);

        return (edges);
    }

