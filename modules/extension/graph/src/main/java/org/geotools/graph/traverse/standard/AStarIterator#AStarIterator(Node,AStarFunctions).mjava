    public AStarIterator(Node source, AStarFunctions afuncs) {

        m_afuncs = afuncs;
        AStarNode asn = new AStarNode(source, afuncs.h(source));
        asn.setG(0);
        setSource(source);
        m_nodemap = new HashMap<>();
        m_nodemap.put(source, asn);
        m_pqueue = new PriorityQueue<>(100, comparator);

        m_pqueue.add(asn);
    }

