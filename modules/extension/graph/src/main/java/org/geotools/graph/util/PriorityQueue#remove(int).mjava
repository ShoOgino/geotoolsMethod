  public void remove(int i) {
    //check to see if item is last in heap
    if (i < m_count) {
      //first do a swap with last element in heap
      swap(i, m_count);
    
      //remove last element
      m_values[m_count--] = null;
    
      //update previous last element
      update(i);
    }
    else {
      //simply remove the last element
      m_values[m_count--] = null;
    }
  }

