    /** @see Node#getEdge(Node) */
    @Override
    public Edge getEdge(Node other) {
        for (Edge m_edge : m_edges) {
            if (m_edge.getNodeA().equals(this) && m_edge.getNodeB().equals(other)
                    || m_edge.getNodeB().equals(this) && m_edge.getNodeA().equals(other))
                return m_edge;
        }
        return (null);
    }

