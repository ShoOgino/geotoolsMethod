    public Object get(Object key) {
        Object obj = null;
        if ((obj = m_map.get(key)) == null) {
            try {
                obj = m_collectionClass.getDeclaredConstructor().newInstance();
            } catch (Exception e) {
                throw new IllegalStateException(e.getClass().getName() + ": " + e.getMessage());
            }
            putItems(key, (Collection) obj);
        }
        return (obj);
    }

