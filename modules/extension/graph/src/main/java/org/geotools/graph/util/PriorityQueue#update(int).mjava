    public int update(int i) {
        // check parent, if value is less, propogate value up
        if (i > 1 && compare(i, i / 2) < 0) {
            return (moveUp(i));
        }

        // check children, if value more, propogate value down
        if ((2 * i <= size() && compare(i, 2) > 0)
                || (2 * i + 1 <= size() && compare(i, 2 * i + 1) > 0)) {
            return (moveDown(i));
        }

        return (i);
    }

