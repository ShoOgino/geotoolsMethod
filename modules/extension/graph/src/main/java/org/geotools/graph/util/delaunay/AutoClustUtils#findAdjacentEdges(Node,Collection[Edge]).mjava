    public static Vector<Edge> findAdjacentEdges(final Node node, final Collection<Edge> edges) {
        Vector<Edge> ret = new Vector<>();
        Iterator<Edge> it = edges.iterator();
        while (it.hasNext()) {
            Edge next = it.next();
            if ((next.getNodeA().equals(node)) || (next.getNodeB().equals(node))) {
                ret.add(next);
            }
        }
        return ret;
    }

