    public void test_initNodes() {
        for (Node mNode : m_nodes) {
            BasicNode n = (BasicNode) mNode;
            n.setVisited(true);
            n.setCount(100);
        }

        m_graph.initNodes();

        for (Node m_node : m_nodes) {
            BasicNode n = (BasicNode) m_node;
            assertTrue(!n.isVisited());
            assertTrue(n.getCount() == 0);
        }
    }

