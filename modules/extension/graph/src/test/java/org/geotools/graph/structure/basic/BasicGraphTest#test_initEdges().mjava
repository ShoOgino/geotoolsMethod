    public void test_initEdges() {
        for (Edge mEdge : m_edges) {
            BasicEdge e = (BasicEdge) mEdge;
            e.setVisited(true);
            e.setCount(100);
        }

        m_graph.initEdges();

        for (Edge m_edge : m_edges) {
            BasicEdge e = (BasicEdge) m_edge;
            assertTrue(!e.isVisited());
            assertTrue(e.getCount() == 0);
        }
    }

