    @Test
    public void test_reverse() {
        List<Node> nodes = new ArrayList<>();
        Walk walk = new Walk();

        for (int i = 0; i < 10; i++) {
            Node n = builder().buildNode();
            nodes.add(n);
            walk.add(n);
        }

        Iterator itr = walk.iterator();
        for (Node n1 : nodes) {
            Node n2 = (Node) itr.next();

            Assert.assertSame(n1, n2);
        }

        walk.reverse();
        itr = walk.iterator();

        for (int i = nodes.size() - 1; i >= 0; i--) {
            Node n1 = nodes.get(i);
            Node n2 = (Node) itr.next();

            Assert.assertSame(n1, n2);
        }
    }

