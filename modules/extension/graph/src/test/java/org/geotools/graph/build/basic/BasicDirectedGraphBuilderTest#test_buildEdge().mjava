    public void test_buildEdge() {
        Node n1 = m_builder.buildNode();
        Node n2 = m_builder.buildNode();

        DirectedEdge de = (DirectedEdge) m_builder.buildEdge(n1, n2);

        assertNotNull(de);
        assertSame(de.getInNode(), n1);
        assertSame(de.getOutNode(), n2);
    }

