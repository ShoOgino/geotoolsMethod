    private URL findURL(OperationType operation) {
        if (operation == null) {
            type = WMTSServiceType.REST;
            return null;
        }
        if (WMTSServiceType.KVP.equals(type)) {
            if (operation.getGet() != null) {
                return operation.getGet();
            }
            return serverURL;

        } else {
            return null;
        }
    }

