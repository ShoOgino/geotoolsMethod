    @Test
    public void testAllSimpleTypes() throws Exception {
        Class xs = XS.class;
        Field[] fields = xs.getFields();

        for (Field field : fields) {
            if (field.getType() != QName.class) {
                continue;
            }

            QName name = (QName) field.get(null);
            xsdSimple(name.getLocalPart());
        }
    }

