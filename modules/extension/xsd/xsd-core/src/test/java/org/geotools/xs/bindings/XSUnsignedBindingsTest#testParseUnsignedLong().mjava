    @Test
    public void testParseUnsignedLong() throws Exception {
        BigDecimal l =
                (BigDecimal)
                        p.parse(
                                new ByteArrayInputStream(
                                        doc("unsignedLong", "12")
                                                .getBytes(Charset.forName("UTF-8"))));
        Assert.assertEquals(12l, l.longValue());
    }

