    Object get(String value) throws Exception {
        try {
            if (get != null) {
                return get.invoke(null, value);
            }
        } catch (Exception e1) {
            try {
                if (valueOf != null) {
                    return valueOf.invoke(null, value);
                }
            } catch (Exception e2) {
                return null;
            }
        }

        return null;
    }

