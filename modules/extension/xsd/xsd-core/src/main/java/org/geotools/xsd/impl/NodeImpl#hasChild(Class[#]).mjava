    @Override
    public boolean hasChild(Class<?> clazz) {
        if (clazz == null) {
            return false;
        }

        for (Node child : children) {
            if (child.getValue() == null) {
                continue;
            }

            if (clazz.isAssignableFrom(child.getValue().getClass())) {
                return true;
            }
        }

        return false;
    }

