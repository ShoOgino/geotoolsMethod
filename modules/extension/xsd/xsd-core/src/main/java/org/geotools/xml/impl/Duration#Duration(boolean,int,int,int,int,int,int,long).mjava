    /** Creates a new instance with the given values. */
    public Duration(
            boolean pNegative,
            int pYears,
            int pMonths,
            int pDays,
            int pHours,
            int pMinutes,
            int pSeconds,
            long pMillis) {
        isNegative = pNegative;
        years = pYears;
        months = pMonths;
        days = pDays;
        hours = pHours;
        minutes = pMinutes;
        seconds = pSeconds;
        millis = pMillis;
    }

