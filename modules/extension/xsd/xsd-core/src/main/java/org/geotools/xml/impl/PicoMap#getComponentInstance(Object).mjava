    public Object getComponentInstance(Object componentKey) {
        if (componentKey == null) {
            return null;
        }

        Object o = get(componentKey);
        if (o instanceof Class) {
            // TODO: instantiate
            Class clazz = (Class) o;
            try {
                return clazz.newInstance();
            } catch (Exception e) {
                throw new RuntimeException(e);
            }
        } else {
            return o;
        }
    }

