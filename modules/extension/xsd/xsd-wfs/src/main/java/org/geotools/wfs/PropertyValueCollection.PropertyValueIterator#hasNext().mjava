        @Override
        public boolean hasNext() {
            if (it == null) {
                return false;
            }
            if (next == null) {
                while(it.hasNext()) {
                    Feature f = (Feature) it.next();
                    if (f.getProperty(descriptor.getName()).getValue() != null) {
                        next = f;
                        break;
                    }
                }
            }

            if (next != null) {
                return true;
            }

            //close the iterator
            it.close();
            it = null;
            return false;
        }

