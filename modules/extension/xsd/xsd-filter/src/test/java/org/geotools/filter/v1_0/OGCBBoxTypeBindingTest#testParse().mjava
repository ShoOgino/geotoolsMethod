    public void testParse() throws Exception {
        FilterMockData.bbox(document, document);

        BBOX box = (BBOX) parse();

        assertEquals("foo", ((PropertyName) box.getExpression1()).getPropertyName());
        assertTrue(
                JTS.equals(
                        new ReferencedEnvelope(0, 1, 0, 1, CRS.decode("EPSG:4326")),
                        box.getBounds(),
                        1e-6));
    }

