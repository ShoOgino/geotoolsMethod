    @Override
    public boolean equals(Object o) {
        if (this == o) {
            return true;
        }
        if (o == null || getClass() != o.getClass()) {
            return false;
        }
        DistanceUnits that = (DistanceUnits) o;
        return Double.compare(that.distance, distance) == 0 && Objects.equals(units, that.units);
    }

