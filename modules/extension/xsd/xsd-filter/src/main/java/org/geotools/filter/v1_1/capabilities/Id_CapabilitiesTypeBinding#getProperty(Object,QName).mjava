    public Object getProperty(Object object, QName name)
        throws Exception {
        IdCapabilities id = (IdCapabilities) object;

        if (OGC.EID.equals(name) && id.hasEID()) {
            return new Object();
        }

        if (OGC.FID.equals(name) && id.hasFID()) {
            return new Object();
        }

        return null;
    }

