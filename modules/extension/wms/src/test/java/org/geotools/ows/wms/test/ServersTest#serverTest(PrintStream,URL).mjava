        } catch (SAXException e) {
            out.println("failed.");
            passed = false;
            out.println("SAXException: " + e.getMessage());
            java.util.logging.Logger.getGlobal().log(java.util.logging.Level.INFO, "", e);
        } finally {
            out.println(request.getFinalURL());
        }

