    @Override
    protected void setUp() throws Exception {
        super.setUp();
        if( wms == null ){
            // do setup once!
            if( serverURL != null ){
                try {
                    wms = new WebMapServer( serverURL );
                    capabilities = wms.getCapabilities();
                }
                catch (Exception eek){
                    serverURL = null;
                    throw eek;
                }
            }
        }        
    }

